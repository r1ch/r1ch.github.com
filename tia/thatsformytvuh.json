input=({subs:[{row:1, start:0.000798611111111111, stop:0.000810185185185185, text0:"Dia.",text1:""},{row:2, start:0.000868055555555556, stop:0.000891203703703704, text0:"English boys don't go",text1:"to school every day."},{row:3, start:0.00194444444444444, stop:0.00197916666666667, text0:"Teacher say this country",text1:"was founded as a utopia."},{row:4, start:0.00197916666666667, stop:0.00201388888888889, text0:"Do you know",text1:"what that word means, Papa?"},{row:5, start:0.00201388888888889, stop:0.00203703703703704, text0:"Well, she says someday",text1:"when the war is over..."},{row:6, start:0.00203703703703704, stop:0.00207175925925926, text0:"...our world will be a paradise.",text1:""},{row:7, start:0.00207175925925926, stop:0.00209490740740741, text0:"And all this you learn in one day?",text1:""},{row:8, start:0.00210648148148148, stop:0.00212962962962963, text0:"And math and science.",text1:""},{row:9, start:0.00212962962962963, stop:0.00217592592592593, text0:"This is too much learning. Tomorrow,",text1:"you will stay home and mend the nets, yes?"},{row:10, start:0.00217592592592593, stop:0.0021875, text0:"No, Papa.",text1:""},{row:11, start:0.0021875, stop:0.00222222222222222, text0:"What? So now you want",text1:"to go to school every day?"},{row:12, start:0.00261574074074074, stop:0.00262731481481481, text0:"- Stay.",text1:"- Papa."},{row:13, start:0.00262731481481481, stop:0.00263888888888889, text0:"- Stay!",text1:"- Papa!"},{row:14, start:0.00306712962962963, stop:0.0030787037037037, text0:"Jassie!",text1:""},{row:15, start:0.0030787037037037, stop:0.00310185185185185, text0:"Solomon.",text1:""},{row:16, start:0.00325231481481482, stop:0.00327546296296296, text0:"Come, come, come.",text1:""},{row:17, start:0.00332175925925926, stop:0.00335648148148148, text0:"Pick it up. Quick.",text1:""},{row:18, start:0.00335648148148148, stop:0.0033912037037037, text0:"- Papa! Papa!",text1:"- Jassie."},{row:19, start:0.0033912037037037, stop:0.00342592592592593, text0:"- Dia, run!",text1:"- Papa!"},{row:20, start:0.00346064814814815, stop:0.00350694444444444, text0:"- Papa! Papa!",text1:"- Dia, run!"},{row:21, start:0.00351851851851852, stop:0.00354166666666667, text0:"Papa!",text1:""},{row:22, start:0.00372685185185185, stop:0.00375, text0:"Bring forward the next one. Bastard.",text1:""},{row:23, start:0.00375, stop:0.00377314814814815, text0:"Bring forward the next one.",text1:""},{row:24, start:0.00378472222222222, stop:0.00381944444444444, text0:"- Long sleeve or short sleeve?",text1:"- No."},{row:25, start:0.00384259259259259, stop:0.00386574074074074, text0:"Young man. You must understand.",text1:""},{row:26, start:0.00386574074074074, stop:0.00390046296296296, text0:"The government wants you to vote, okay?",text1:""},{row:27, start:0.00390046296296296, stop:0.00393518518518519, text0:"They gonna tell you say,",text1:"'The future is in your hands.'"},{row:28, start:0.00394675925925926, stop:0.00396990740740741, text0:"We now the future.",text1:""},{row:29, start:0.00396990740740741, stop:0.00399305555555556, text0:"So we take your hands.",text1:""},{row:30, start:0.00399305555555556, stop:0.0040162037037037, text0:"- No.",text1:"- No more hands, no more voting."},{row:31, start:0.00403935185185185, stop:0.0040625, text0:"Chop him.",text1:""},{row:32, start:0.00409722222222222, stop:0.00414351851851852, text0:"Spread the word.",text1:"The Revolutionary United Front is coming."},{row:33, start:0.00416666666666667, stop:0.00421296296296296, text0:"- RUF!",text1:"- RUF! RUF!"},{row:34, start:0.00423611111111111, stop:0.00428240740740741, text0:"Bring forward the next one.",text1:"Bring him forward. Bring him forward."},{row:35, start:0.00430555555555556, stop:0.0043287037037037, text0:"Long sleeve or short sleeve?",text1:""},{row:36, start:0.00435185185185185, stop:0.00436342592592593, text0:"- Short sleeve!",text1:"- Chop him."},{row:37, start:0.004375, stop:0.00439814814814815, text0:"Hold on, hold on. Wait, wait, wait.",text1:""},{row:38, start:0.0044212962962963, stop:0.00444444444444444, text0:"Not this one. Look at him.",text1:""},{row:39, start:0.00445601851851852, stop:0.00446759259259259, text0:"- Put him in the truck.",text1:"- Let's go."},{row:40, start:0.00446759259259259, stop:0.00449074074074074, text0:"Bring him to the mines.",text1:""},{row:41, start:0.00449074074074074, stop:0.00451388888888889, text0:"He can work. He can work.",text1:"Move, move, move."},{row:42, start:0.00451388888888889, stop:0.00453703703703704, text0:"Next one, next one, next one.",text1:""},{row:43, start:0.00460648148148148, stop:0.00461805555555556, text0:"Throughout the history of Africa...",text1:""},{row:44, start:0.00462962962962963, stop:0.00465277777777778, text0:"...whenever a substance",text1:"of value is found..."},{row:45, start:0.00467592592592593, stop:0.00471064814814815, text0:"...the locals die",text1:"in great number and in misery."},{row:46, start:0.00471064814814815, stop:0.00478009259259259, text0:"Now, this was true of ivory,",text1:"rubber, gold and oil."},{row:47, start:0.00478009259259259, stop:0.00479166666666667, text0:"It is now true of diamonds.",text1:""},{row:48, start:0.00481481481481482, stop:0.00486111111111111, text0:"According to a devastating report",text1:"by Global Witness..."},{row:49, start:0.00486111111111111, stop:0.00491898148148148, text0:"...these stones are being used",text1:"to purchase arms and finance civil war."},{row:50, start:0.00491898148148148, stop:0.0049537037037037, text0:"We must act to prohibit...",text1:""},{row:51, start:0.0049537037037037, stop:0.005, text0:"...the direct or indirect import",text1:"of all rough diamonds from conflict zones."},{row:52, start:0.00508101851851852, stop:0.00509259259259259, text0:"May I remind you...",text1:""},{row:53, start:0.00509259259259259, stop:0.00515046296296296, text0:"...that the U.S. is responsible for two-thirds",text1:"of all diamond purchases worldwide..."},{row:54, start:0.00515046296296296, stop:0.00519675925925926, text0:"...and I don't anticipate",text1:"that demand diminishing."},{row:55, start:0.00519675925925926, stop:0.00521990740740741, text0:"We must remember",text1:"that these stones..."},{row:56, start:0.00521990740740741, stop:0.00527777777777778, text0:"...comprise only a small percentage",text1:"of the legitimate diamond industry..."},{row:57, start:0.00527777777777778, stop:0.00533564814814815, text0:"...whose trade is crucial to the economies",text1:"of many emerging nations."},{row:58, start:0.00534722222222222, stop:0.00538194444444445, text0:"The Freetown government",text1:"and their white masters..."},{row:59, start:0.00538194444444445, stop:0.00541666666666667, text0:"...have raped your land",text1:"to feed their greed."},{row:60, start:0.00541666666666667, stop:0.00543981481481481, text0:"RUF have freed you.",text1:""},{row:61, start:0.00543981481481481, stop:0.00547453703703704, text0:"- No more slave and master here.",text1:"- It's true."},{row:62, start:0.00548611111111111, stop:0.00554398148148148, text0:"Current estimates are that conflict stones",text1:"account for only 15 percent of the market."},{row:63, start:0.00554398148148148, stop:0.00556712962962963, text0:"But in a multi-billion-dollar-a-year",text1:"industry..."},{row:64, start:0.00556712962962963, stop:0.00560185185185185, text0:"...that means hundreds of millions",text1:"of dollars are available..."},{row:65, start:0.00561342592592593, stop:0.00563657407407407, text0:"...for weapons in these conflict zones.",text1:""},{row:66, start:0.00563657407407407, stop:0.00565972222222222, text0:"RUF is fighting for the people!",text1:""},{row:67, start:0.00565972222222222, stop:0.00569444444444444, text0:"RUF is fighting for Sierra Leone!",text1:""},{row:68, start:0.00569444444444444, stop:0.00575231481481481, text0:"Any bastard think he would joke",text1:"with me diamond, I go cut he throat."},{row:69, start:0.00577546296296296, stop:0.00578703703703704, text0:"Stop!",text1:""},{row:70, start:0.00581018518518519, stop:0.00585648148148148, text0:"And we are joined today",text1:"by members of that diamond industry..."},{row:71, start:0.00585648148148148, stop:0.00590277777777778, text0:"...who wholeheartedly",text1:"endorse your proposal."},{row:72, start:0.00590277777777778, stop:0.00594907407407407, text0:"I hope you join me",text1:"in applauding their presence here."},{row:73, start:0.00594907407407407, stop:0.0059837962962963, text0:"Mr. Van De Kaap and Mr. Simmons.",text1:""},{row:74, start:0.00608796296296296, stop:0.00611111111111111, text0:"Give it to me.",text1:""},{row:75, start:0.00649305555555555, stop:0.00652777777777778, text0:"Don't worry. We'll be fine, huh?",text1:""},{row:76, start:0.00653935185185185, stop:0.00658564814814815, text0:"Listen. You take off",text1:"as soon as I'm out the door."},{row:77, start:0.00658564814814815, stop:0.00662037037037037, text0:"You don't land",text1:"until I raise you on the sat phone."},{row:78, start:0.00663194444444444, stop:0.00664351851851852, text0:"You be careful, Danny.",text1:""},{row:79, start:0.00665509259259259, stop:0.00670138888888889, text0:"Don't worry. They want these guns",text1:"too much to do anything stupid."},{row:80, start:0.00686342592592593, stop:0.00688657407407407, text0:"Where is Commander Zero?",text1:""},{row:81, start:0.00690972222222222, stop:0.00693287037037037, text0:"I am Captain Rambo.",text1:""},{row:82, start:0.00694444444444444, stop:0.00699074074074074, text0:"Right. Right, I've seen your films, huh?",text1:""},{row:83, start:0.00702546296296296, stop:0.00704861111111111, text0:"I'm looking for Commander Zero, huh?",text1:""},{row:84, start:0.00706018518518518, stop:0.00707175925925926, text0:"He's inside. You talk to me.",text1:""},{row:85, start:0.00708333333333333, stop:0.00710648148148148, text0:"You are here to help us",text1:"in our struggle against the government."},{row:86, start:0.00711805555555556, stop:0.0071412037037037, text0:"I'm here to do business",text1:"with Commander Zero, all right?"},{row:87, start:0.00733796296296296, stop:0.00734953703703704, text0:"All right.",text1:""},{row:88, start:0.00734953703703704, stop:0.00739583333333333, text0:"You are the man, huh?",text1:"You are the man."},{row:89, start:0.00744212962962963, stop:0.0074537037037037, text0:"Here, huh?",text1:""},{row:90, start:0.00759259259259259, stop:0.00762731481481482, text0:"Hey. I'm talking to you! Stop!",text1:""},{row:91, start:0.00762731481481482, stop:0.00765046296296296, text0:"- Commander Zero.",text1:"- Hey."},{row:92, start:0.00765046296296296, stop:0.00768518518518518, text0:"- I go kill you, eh?",text1:"- It's fine, huh? It's fine."},{row:93, start:0.00768518518518518, stop:0.00773148148148148, text0:"Commander Zero. Commander Zero.",text1:""},{row:94, start:0.00773148148148148, stop:0.0077662037037037, text0:"Mr. Archer, come here.",text1:""},{row:95, start:0.00784722222222222, stop:0.00787037037037037, text0:"You get something for me?",text1:""},{row:96, start:0.00787037037037037, stop:0.00789351851851852, text0:"And yourself get something for me, huh?",text1:""},{row:97, start:0.00791666666666667, stop:0.00795138888888889, text0:"Are you craze?",text1:"You get for pay me first for them."},{row:98, start:0.00815972222222222, stop:0.00818287037037037, text0:"This no fine, huh?",text1:""},{row:99, start:0.00818287037037037, stop:0.00819444444444445, text0:"This no fine.",text1:""},{row:100, start:0.00827546296296296, stop:0.00829861111111111, text0:"...and their new weapons, huh?",text1:""},{row:101, start:0.00829861111111111, stop:0.00832175925925926, text0:"Maybe I could kill you",text1:"and take what you bring."},{row:102, start:0.00833333333333333, stop:0.00834490740740741, text0:"Then you get one more dead body...",text1:""},{row:103, start:0.00835648148148148, stop:0.00837962962962963, text0:"...instead of airplane",text1:"way full with grenade launchers."},{row:104, start:0.0083912037037037, stop:0.00841435185185185, text0:"So I'm think I'm gonna go",text1:"to them government."},{row:105, start:0.00842592592592593, stop:0.00844907407407407, text0:"Them government,",text1:"at least them go pay me, huh?"},{row:106, start:0.00844907407407407, stop:0.0084837962962963, text0:"Wait, wait. Wait, my friend.",text1:""},{row:107, start:0.00853009259259259, stop:0.00855324074074074, text0:"This is the thing you want? Here.",text1:""},{row:108, start:0.00856481481481482, stop:0.00858796296296296, text0:"So many I not know",text1:"what to do with them all."},{row:109, start:0.00858796296296296, stop:0.00862268518518518, text0:"Hey, Archer, next time,",text1:"you bring satellite TV, eh?"},{row:110, start:0.00863425925925926, stop:0.00865740740740741, text0:"- I wanna see Baywatch.",text1:"- Yeah."},{row:111, start:0.00866898148148148, stop:0.00868055555555556, text0:"Yeah. Yeah.",text1:""},{row:112, start:0.00895833333333333, stop:0.00896990740740741, text0:"Hello, hello, hello.",text1:""},{row:113, start:0.00899305555555555, stop:0.0090162037037037, text0:"Papers, please.",text1:""},{row:114, start:0.0090162037037037, stop:0.00903935185185185, text0:"Danny Archer, National Geographic.",text1:""},{row:115, start:0.00907407407407407, stop:0.00909722222222222, text0:"You crossing into Liberia.",text1:""},{row:116, start:0.00909722222222222, stop:0.00913194444444444, text0:"I've got a letter from the minister of interior",text1:"and a Liberian visa as well."},{row:117, start:0.00914351851851852, stop:0.00916666666666667, text0:"I'm doing a story on the Marrakaz,",text1:"who, as you know..."},{row:118, start:0.00916666666666667, stop:0.00921296296296296, text0:"...are allowed to cross the border",text1:"in order to get into grazing grounds."},{row:119, start:0.00921296296296296, stop:0.00923611111111111, text0:"You ever read National Geographic, huh?",text1:""},{row:120, start:0.00972222222222222, stop:0.00974537037037037, text0:"- You're under arrest for smuggling.",text1:"- No, no, no."},{row:121, start:0.00974537037037037, stop:0.00978009259259259, text0:"Listen here, my friend.",text1:"Listen here, all right?"},{row:122, start:0.00978009259259259, stop:0.00980324074074074, text0:"I am good friends",text1:"with Colonel Coetzee."},{row:123, start:0.00981481481481481, stop:0.00986111111111111, text0:"Colonel Coetzee would not be pleased",text1:"that you've interfered with his business."},{row:124, start:0.00986111111111111, stop:0.00988425925925926, text0:"Now, you know who I am, all right?",text1:""},{row:125, start:0.00988425925925926, stop:0.00991898148148148, text0:"You don't wanna make trouble for me",text1:"or my friends, all right?"},{row:126, start:0.00991898148148148, stop:0.00996527777777778, text0:"Now, look, why don't I just",text1:"look the other way, all right?"},{row:127, start:0.00996527777777778, stop:0.01, text0:"You take one or two of those stones",text1:"and get something lacquer for the wife..."},{row:128, start:0.0100115740740741, stop:0.0100462962962963, text0:"...or maybe the mistress, all right?",text1:"We'll solve the problem right here, huh?"},{row:129, start:0.0100578703703704, stop:0.0100925925925926, text0:"We'll go back to Freetown",text1:"and ask Colonel Coetzee..."},{row:130, start:0.0100925925925926, stop:0.0101273148148148, text0:"...how he wishes",text1:"to thank his business associate."},{row:131, start:0.0101273148148148, stop:0.010150462962963, text0:"Guys, take him away.",text1:""},{row:132, start:0.0106828703703704, stop:0.0107175925925926, text0:"Hey! What the fuck?",text1:"What's going on there?"},{row:133, start:0.0107175925925926, stop:0.0107407407407407, text0:"Come on! Fix that shit now!",text1:""},{row:134, start:0.0107407407407407, stop:0.010775462962963, text0:"Wasting my time here!",text1:"What in the shit is that?"},{row:135, start:0.010787037037037, stop:0.0108217592592593, text0:"Fuck that stuff! Fix that shit!",text1:"Damn, now need to work."},{row:136, start:0.0108217592592593, stop:0.0108449074074074, text0:"- Can't work. What's going on there?",text1:"- Boss."},{row:137, start:0.0108449074074074, stop:0.0108796296296296, text0:"- Fix that thing.",text1:"- I wanna go toilet. I must go now, now."},{row:138, start:0.0108796296296296, stop:0.0109259259259259, text0:"- I can't hold it in, boss.",text1:"- Go, go."},{row:139, start:0.0109259259259259, stop:0.0109490740740741, text0:"Get to work. Go to work, go to work.",text1:""},{row:140, start:0.0109490740740741, stop:0.0109953703703704, text0:"Fuckheads wasting my time now.",text1:"How am I work in this shit?"},{row:141, start:0.0110648148148148, stop:0.0110763888888889, text0:"Stop!",text1:""},{row:142, start:0.011099537037037, stop:0.0111226851851852, text0:"Spread your fingers.",text1:""},{row:143, start:0.0111342592592593, stop:0.0111574074074074, text0:"Open.",text1:""},{row:144, start:0.0113310185185185, stop:0.0113541666666667, text0:"Make it quick.",text1:""},{row:145, start:0.0117708333333333, stop:0.0117824074074074, text0:"Give it to me.",text1:""},{row:146, start:0.0118171296296296, stop:0.0118402777777778, text0:"Give it to me.",text1:""},{row:147, start:0.0118402777777778, stop:0.0118634259259259, text0:"The government troops are coming!",text1:""},{row:148, start:0.0119212962962963, stop:0.0119560185185185, text0:"Go, go, go--!",text1:""},{row:149, start:0.0123148148148148, stop:0.0123263888888889, text0:"No, no, no!",text1:""},{row:150, start:0.0123263888888889, stop:0.0123611111111111, text0:"No! I'm not a rebel!",text1:""},{row:151, start:0.0128125, stop:0.0128472222222222, text0:"RUF is coming! RUF is coming!",text1:""},{row:152, start:0.0128472222222222, stop:0.0128935185185185, text0:"RUF is coming! RUF is coming!",text1:"RUF is coming!"},{row:153, start:0.0128935185185185, stop:0.0129398148148148, text0:"RUF is coming! RUF is coming!",text1:"RUF is coming!"},{row:154, start:0.0129398148148148, stop:0.0129861111111111, text0:"RUF is coming! RUF is coming!",text1:"RUF is coming!"},{row:155, start:0.0130671296296296, stop:0.0130902777777778, text0:"RUF! RUF! RUF!",text1:""},{row:156, start:0.0130902777777778, stop:0.013125, text0:"RUF! RUF! RUF!",text1:""},{row:157, start:0.013125, stop:0.0131597222222222, text0:"RUF! RUF! RUF!",text1:""},{row:158, start:0.0131597222222222, stop:0.0131828703703704, text0:"RUF! RUF! RUF!",text1:""},{row:159, start:0.0132523148148148, stop:0.013275462962963, text0:"Hey.",text1:""},{row:160, start:0.013275462962963, stop:0.0132986111111111, text0:"What did you do with it?",text1:""},{row:161, start:0.0132986111111111, stop:0.0133217592592593, text0:"I'm talking to you, you hear me?",text1:""},{row:162, start:0.0133449074074074, stop:0.0133796296296296, text0:"Where the diamond? The pink one.",text1:""},{row:163, start:0.0133796296296296, stop:0.0134143518518519, text0:"- I saw you take it.",text1:"- You're mad. I do not know you."},{row:164, start:0.0134143518518519, stop:0.0134606481481481, text0:"Liar! I saw it with my own eyes.",text1:""},{row:165, start:0.0134722222222222, stop:0.0135185185185185, text0:"This big. Biggest I have ever seen.",text1:""},{row:166, start:0.0135300925925926, stop:0.0135763888888889, text0:"My brothers, I go give $1000...",text1:""},{row:167, start:0.0135763888888889, stop:0.0136111111111111, text0:"...to the man who cuts the diamond",text1:"out of this bastard."},{row:168, start:0.0136111111111111, stop:0.0136574074074074, text0:"You the craze! You the craze!",text1:""},{row:169, start:0.0136574074074074, stop:0.0136805555555556, text0:"Where is the diamond?",text1:""},{row:170, start:0.0136805555555556, stop:0.0137152777777778, text0:"Where is the diamond?",text1:"Do you see a diamond?"},{row:171, start:0.0137268518518519, stop:0.0137847222222222, text0:"You devils have taken my family!",text1:"My home! I done lost everything!"},{row:172, start:0.0137962962962963, stop:0.0138310185185185, text0:"Here, look. What is left?",text1:""},{row:173, start:0.0138425925925926, stop:0.0138541666666667, text0:"What is left?",text1:""},{row:174, start:0.0138657407407407, stop:0.0138888888888889, text0:"If there is a diamond,",text1:"you are the one who has taken it!"},{row:175, start:0.0138888888888889, stop:0.013912037037037, text0:"Liar! The man is a liar!",text1:""},{row:176, start:0.0139236111111111, stop:0.0139699074074074, text0:"I know your name, Solomon Vandy!",text1:"You have a name!"},{row:177, start:0.0139699074074074, stop:0.0140162037037037, text0:"You have a family!",text1:"And I'll catch them!"},{row:178, start:0.0140162037037037, stop:0.0140393518518519, text0:"And I'll find them!",text1:""},{row:179, start:0.0142361111111111, stop:0.0142708333333333, text0:"Please tell me you brought me",text1:"cigarettes, bru, please."},{row:180, start:0.0142708333333333, stop:0.0142939814814815, text0:"I just have to tell you, Danny.",text1:""},{row:181, start:0.0142939814814815, stop:0.0143287037037037, text0:"The colonel's not going to be happy",text1:"about you losing the diamonds."},{row:182, start:0.0143287037037037, stop:0.0143634259259259, text0:"Forget the colonel.",text1:"There's a fisherman in there, all right?"},{row:183, start:0.0143634259259259, stop:0.0143981481481481, text0:"He might've found a pink. Big one.",text1:""},{row:184, start:0.0143981481481481, stop:0.0144212962962963, text0:"What about the colonel?",text1:"He'll want that stone."},{row:185, start:0.0144212962962963, stop:0.0144560185185185, text0:"Screw the colonel.",text1:"I'll contact our friends in London."},{row:186, start:0.0144560185185185, stop:0.0145023148148148, text0:"If this stone is as big as I think,",text1:"we're gonna need a real buyer, all right?"},{row:187, start:0.0145023148148148, stop:0.014537037037037, text0:"What? What?",text1:""},{row:188, start:0.014537037037037, stop:0.0145717592592593, text0:"London wants nothing more",text1:"to do with you."},{row:189, start:0.0145717592592593, stop:0.0146180555555556, text0:"'Arrest in Sierra Leone of Daniel Archer,",text1:"former mercenary and smuggler...'"},{row:190, start:0.0146296296296296, stop:0.0146527777777778, text0:"'Retains close ties",text1:"to Van De Kaap diamond cartel."},{row:191, start:0.0146527777777778, stop:0.0146990740740741, text0:"Sources say they deny",text1:"any affiliation whatso--'"},{row:192, start:0.0146990740740741, stop:0.0147222222222222, text0:"Fucking hell.",text1:""},{row:193, start:0.0147337962962963, stop:0.0147569444444444, text0:"Listen, his name",text1:"is Solomon Vandy, all right?"},{row:194, start:0.0147685185185185, stop:0.0148032407407407, text0:"Van De Kaap is gonna want that stone,",text1:"whatever they say. You just get him out."},{row:195, start:0.0148032407407407, stop:0.0148263888888889, text0:"Get him out.",text1:""},{row:196, start:0.0148611111111111, stop:0.0149189814814815, text0:"Yeah, can you tell Mr. Simmons",text1:"that Danny Archer called. Right."},{row:197, start:0.0149189814814815, stop:0.0149652777777778, text0:"Archer. A-R-C-H-E-R.",text1:""},{row:198, start:0.0152430555555556, stop:0.0152662037037037, text0:"Afternoon, darling, huh?",text1:""},{row:199, start:0.0152662037037037, stop:0.0153125, text0:"Big white man all alone.",text1:"That is no fine."},{row:200, start:0.0153125, stop:0.0153703703703704, text0:"- I'm safe, huh? No HIV.",text1:"- Yeah, yeah. I've heard that one before."},{row:201, start:0.0153703703703704, stop:0.0154050925925926, text0:"Archer, I know where to get",text1:"nice baby diamond just for you."},{row:202, start:0.0154050925925926, stop:0.0154282407407407, text0:"- Not now, next time.",text1:"- Come on, Archer."},{row:203, start:0.0154282407407407, stop:0.015462962962963, text0:"I said not now, next time. Bugger off.",text1:""},{row:204, start:0.0160648148148148, stop:0.016087962962963, text0:"Solomon Vandy?",text1:""},{row:205, start:0.016099537037037, stop:0.0161226851851852, text0:"What do you want?",text1:""},{row:206, start:0.0161226851851852, stop:0.0161574074074074, text0:"It seems you made a friend in prison.",text1:""},{row:207, start:0.0164467592592593, stop:0.0164699074074074, text0:"- Hey.",text1:"- How the body, Mr. Archer?"},{row:208, start:0.0164699074074074, stop:0.0164930555555556, text0:"The body fine, M'Ed.",text1:""},{row:209, start:0.0165509259259259, stop:0.0165740740740741, text0:"So you want something nice, huh?",text1:""},{row:210, start:0.0165740740740741, stop:0.0166087962962963, text0:"The best you can get ahold of, all right?",text1:""},{row:211, start:0.0166087962962963, stop:0.0166550925925926, text0:"Take me a few days,",text1:"but I go find you something special."},{row:212, start:0.0166550925925926, stop:0.0166898148148148, text0:"All right. You hear",text1:"they took Marampa yesterday?"},{row:213, start:0.0167013888888889, stop:0.016724537037037, text0:"- Really?",text1:"- Now, come on."},{row:214, start:0.016724537037037, stop:0.0167476851851852, text0:"You know every move they make.",text1:"When are they gonna hit us?"},{row:215, start:0.0167592592592593, stop:0.0168055555555556, text0:"Oh, I imagine they come",text1:"do some shopping very soon."},{row:216, start:0.0168055555555556, stop:0.0168402777777778, text0:"Yeah, yeah.",text1:"Got any smokes back there?"},{row:217, start:0.0168518518518519, stop:0.0168634259259259, text0:"Yeah.",text1:""},{row:218, start:0.0169675925925926, stop:0.0169907407407407, text0:"Who's that, huh?",text1:""},{row:219, start:0.0170023148148148, stop:0.017025462962963, text0:"All right.",text1:""},{row:220, start:0.0170949074074074, stop:0.0171412037037037, text0:"- Can I offer you a cigarette?",text1:"- Oh, no, thanks."},{row:221, start:0.0171412037037037, stop:0.0171643518518519, text0:"You listening to this?",text1:""},{row:222, start:0.0171643518518519, stop:0.0171990740740741, text0:"I am profoundly sorry.",text1:""},{row:223, start:0.0171990740740741, stop:0.0172337962962963, text0:"The world is falling apart,",text1:"and all we hear about is Blowjobgate."},{row:224, start:0.0172453703703704, stop:0.0172569444444444, text0:"I believe he's sorry.",text1:""},{row:225, start:0.0172685185185185, stop:0.0173032407407407, text0:"When was the last time the world",text1:"wasn't falling apart, huh?"},{row:226, start:0.0173263888888889, stop:0.0173726851851852, text0:"A cynic. Why don't you sit down",text1:"and make me miserable?"},{row:227, start:0.0173958333333333, stop:0.0174305555555556, text0:"- Danny Archer.",text1:"- Maddy Bowen."},{row:228, start:0.0174305555555556, stop:0.0174537037037037, text0:"- Pleased to meet you.",text1:"- Nice to meet you."},{row:229, start:0.0174537037037037, stop:0.0174768518518519, text0:"- American, huh?",text1:"- Guilty."},{row:230, start:0.0174884259259259, stop:0.0175115740740741, text0:"Well, Americans usually are.",text1:""},{row:231, start:0.0175115740740741, stop:0.0175462962962963, text0:"Says the white South African?",text1:""},{row:232, start:0.0175578703703704, stop:0.0175810185185185, text0:"I'm from Rhodesia.",text1:""},{row:233, start:0.0176041666666667, stop:0.0176273148148148, text0:"We say Zimbabwe now,",text1:"don't we?"},{row:234, start:0.0176273148148148, stop:0.017662037037037, text0:"- Do we?",text1:"- Last time I checked."},{row:235, start:0.0176736111111111, stop:0.0176967592592593, text0:"Don't tell me,",text1:"you're here to make a difference."},{row:236, start:0.0176967592592593, stop:0.0177314814814815, text0:"- And you're here to make a buck?",text1:"- I'm here for lack of a better idea."},{row:237, start:0.0177430555555556, stop:0.0177662037037037, text0:"- That's a shame.",text1:"- Not really."},{row:238, start:0.0177662037037037, stop:0.0177893518518519, text0:"Peace Corps types only",text1:"stay around long enough..."},{row:239, start:0.0177893518518519, stop:0.0178125, text0:"...to realize they're not helping anyone.",text1:""},{row:240, start:0.0178240740740741, stop:0.0178356481481481, text0:"Government only wants",text1:"to stay in power..."},{row:241, start:0.0178472222222222, stop:0.0178703703703704, text0:"...until they've stolen enough",text1:"to go into exile somewhere else."},{row:242, start:0.0178819444444444, stop:0.0179050925925926, text0:"And the rebels, they're not sure",text1:"they want to take over."},{row:243, start:0.0179050925925926, stop:0.0179398148148148, text0:"Otherwise,",text1:"they'd have to govern this mess."},{row:244, start:0.0179398148148148, stop:0.017962962962963, text0:"- But TIA, right, M'Ed?",text1:"- TIA."},{row:245, start:0.017962962962963, stop:0.0179861111111111, text0:"What's TIA?",text1:""},{row:246, start:0.0179861111111111, stop:0.0180324074074074, text0:"- 'This is Africa,' huh?",text1:"- Right."},{row:247, start:0.0180439814814815, stop:0.0180671296296296, text0:"- You want another?",text1:"- Sure."},{row:248, start:0.0181134259259259, stop:0.0181481481481481, text0:"So which one are you?",text1:""},{row:249, start:0.0181597222222222, stop:0.0181944444444444, text0:"- Smuggler?",text1:"- Am I?"},{row:250, start:0.0181944444444444, stop:0.0182291666666667, text0:"Somehow you don't strike me",text1:"as the UNICEF type."},{row:251, start:0.0182407407407407, stop:0.0182638888888889, text0:"How about soldier of fortune?",text1:""},{row:252, start:0.018275462962963, stop:0.0182986111111111, text0:"Or is that too much of a cliché?",text1:""},{row:253, start:0.0183101851851852, stop:0.0183217592592593, text0:"Diamonds?",text1:""},{row:254, start:0.0183333333333333, stop:0.0183680555555556, text0:"- If I told you I was a missionary...?",text1:"- For Van De Kaap?"},{row:255, start:0.0183912037037037, stop:0.0184259259259259, text0:"Better watch that type of talk,",text1:"Miss Bowen."},{row:256, start:0.0184259259259259, stop:0.0184606481481481, text0:"In America, it's bling-bling,",text1:"but out here, it's bling-bang, huh?"},{row:257, start:0.0184606481481481, stop:0.0184953703703704, text0:"I wouldn't want you",text1:"getting in any trouble."},{row:258, start:0.0185069444444444, stop:0.0185416666666667, text0:"How much trouble",text1:"do you think I would get into..."},{row:259, start:0.0185416666666667, stop:0.0185763888888889, text0:"...for talking about blood diamonds?",text1:""},{row:260, start:0.0186111111111111, stop:0.0186458333333333, text0:"- You're a journalist.",text1:"- That's right."},{row:261, start:0.0186921296296296, stop:0.0187152777777778, text0:"Piss off, huh?",text1:""},{row:262, start:0.0187615740740741, stop:0.0187962962962963, text0:"For five years, this country has reported",text1:"almost no diamond exports..."},{row:263, start:0.0187962962962963, stop:0.0188425925925926, text0:"...while Liberia, right next door,",text1:"has exported 2 billion dollars' worth."},{row:264, start:0.0188425925925926, stop:0.0188773148148148, text0:"Very odd, considering that Liberia",text1:"has no diamonds to speak of."},{row:265, start:0.0188773148148148, stop:0.0188888888888889, text0:"I'm shocked.",text1:""},{row:266, start:0.018900462962963, stop:0.0189351851851852, text0:"Van De Kaap denies trading blood",text1:"diamonds, but admits it's impossible..."},{row:267, start:0.0189351851851852, stop:0.0189814814814815, text0:"...to tell where a diamond comes from",text1:"unless their smuggler gets caught."},{row:268, start:0.0189930555555556, stop:0.0190046296296296, text0:"What do you want from me, huh?",text1:""},{row:269, start:0.0190046296296296, stop:0.0190625, text0:"You know all about Van De Kaap.",text1:"Help me out. Off the record."},{row:270, start:0.0190625, stop:0.0191203703703704, text0:"Well, off the record, I like to get kissed",text1:"before I get fucked, huh?"},{row:271, start:0.0191435185185185, stop:0.0191550925925926, text0:"Shit.",text1:""},{row:272, start:0.0195486111111111, stop:0.0195833333333333, text0:"- Cordell. How is it, Shona?",text1:"- Long time, pal."},{row:273, start:0.0195833333333333, stop:0.0196296296296296, text0:"- How's Alice?",text1:"- She's well, thanks."},{row:274, start:0.0196296296296296, stop:0.0196527777777778, text0:"Tim must be in school, right?",text1:""},{row:275, start:0.0196527777777778, stop:0.0196759259259259, text0:"- Big kids, big problems, you know?",text1:"- Yeah, yeah."},{row:276, start:0.0196875, stop:0.0197106481481481, text0:"I heard you got into a spot of trouble",text1:"in the bush, huh?"},{row:277, start:0.0197106481481481, stop:0.0197453703703704, text0:"Well, you know the bush, right, bru?",text1:""},{row:278, start:0.0197453703703704, stop:0.0197800925925926, text0:"- Company doing well?",text1:"- Can't complain."},{row:279, start:0.0197800925925926, stop:0.0198148148148148, text0:"Eleven wars in the continent.",text1:"We're keeping busy."},{row:280, start:0.0198148148148148, stop:0.019837962962963, text0:"So, what, you're here on holiday, huh?",text1:""},{row:281, start:0.0198611111111111, stop:0.0198842592592593, text0:"- How's the colonel?",text1:"- He sends his regards."},{row:282, start:0.0198842592592593, stop:0.0199305555555556, text0:"Good, good. Score us a loose, huh?",text1:""},{row:283, start:0.0199421296296296, stop:0.0199652777777778, text0:"Smoking will kill you, bru.",text1:""},{row:284, start:0.0199652777777778, stop:0.0199884259259259, text0:"Only if I live, right?",text1:""},{row:285, start:0.0200462962962963, stop:0.0200694444444444, text0:"That's for breaking my TV, bru.",text1:""},{row:286, start:0.0201273148148148, stop:0.020162037037037, text0:"Tell the colonel he'll get his money.",text1:"I'll come see him soon."},{row:287, start:0.0202430555555556, stop:0.0202546296296296, text0:"Ten more.",text1:""},{row:288, start:0.0202662037037037, stop:0.0202893518518519, text0:"Come. Come.",text1:""},{row:289, start:0.0203703703703704, stop:0.0203819444444444, text0:"Go on.",text1:""},{row:290, start:0.0205439814814815, stop:0.0205902777777778, text0:"Dia. Dia. Come here.",text1:"Get away from there."},{row:291, start:0.0206018518518519, stop:0.020625, text0:"Go. Go, Mama.",text1:""},{row:292, start:0.0206365740740741, stop:0.0206597222222222, text0:"Stay down.",text1:""},{row:293, start:0.0208680555555556, stop:0.0208796296296296, text0:"Excuse me,",text1:"my name's Solomon Vandy--"},{row:294, start:0.0208796296296296, stop:0.0209143518518519, text0:"- Check the list.",text1:"- I have checked the list."},{row:295, start:0.0209143518518519, stop:0.0209606481481481, text0:"- File papers at the Office for Refugees.",text1:"- Sir, I have filed papers with the office."},{row:296, start:0.0209606481481481, stop:0.0209837962962963, text0:"Then God help you,",text1:"because I can't."},{row:297, start:0.0209953703703704, stop:0.0210069444444444, text0:"- Please, sir--",text1:"- Next."},{row:298, start:0.0210069444444444, stop:0.0210300925925926, text0:"- Sir-- Sir, no, please.",text1:"- Hey, hey. Me, me, me."},{row:299, start:0.0210300925925926, stop:0.0210763888888889, text0:"Husband here. One year we look.",text1:""},{row:300, start:0.0211226851851852, stop:0.0211574074074074, text0:"Hurry, Mom. Go.",text1:""},{row:301, start:0.0212152777777778, stop:0.0212268518518519, text0:"Dia! Dia!",text1:""},{row:302, start:0.0212268518518519, stop:0.0212384259259259, text0:"- Jassie!",text1:"- Dia!"},{row:303, start:0.0212384259259259, stop:0.0212615740740741, text0:"- Mama!",text1:"- Dia!"},{row:304, start:0.0212615740740741, stop:0.0212847222222222, text0:"Jassie! Mama!",text1:""},{row:305, start:0.0215509259259259, stop:0.0215740740740741, text0:"He's been waiting.",text1:"What took you?"},{row:306, start:0.0215856481481481, stop:0.0216203703703704, text0:"What are you gonna do,",text1:"shoot me right here, huh, Shona?"},{row:307, start:0.0216319444444444, stop:0.0216666666666667, text0:"Now, don't be a wanker, man. Get in.",text1:""},{row:308, start:0.022037037037037, stop:0.0220601851851852, text0:"- Hello, Danny.",text1:"- Colonel."},{row:309, start:0.0220601851851852, stop:0.0220833333333333, text0:"You look well.",text1:""},{row:310, start:0.0220833333333333, stop:0.0221296296296296, text0:"Clean living and a pure heart,",text1:"I suppose, huh?"},{row:311, start:0.0221296296296296, stop:0.0221527777777778, text0:"Walk with me.",text1:""},{row:312, start:0.0221643518518519, stop:0.0221990740740741, text0:"It seems the rebels have taken back",text1:"the diamond fields, Danny."},{row:313, start:0.0221990740740741, stop:0.0222337962962963, text0:"The government",text1:"of Sierra Leone has contracted us..."},{row:314, start:0.0222337962962963, stop:0.0222569444444444, text0:"...to go in there",text1:"and take the rebels down."},{row:315, start:0.0222569444444444, stop:0.0222800925925926, text0:"So you sell the rebels weapons...",text1:""},{row:316, start:0.0222800925925926, stop:0.0223032407407407, text0:"...the government hires you",text1:"when they use them? Nice, sir."},{row:317, start:0.0223148148148148, stop:0.022349537037037, text0:"I assume you're asking",text1:"for mining concessions, huh?"},{row:318, start:0.022349537037037, stop:0.0223842592592593, text0:"We save the government,",text1:"they show their gratitude."},{row:319, start:0.0223842592592593, stop:0.0224189814814815, text0:"And you get rich, right?",text1:""},{row:320, start:0.0224652777777778, stop:0.0224768518518519, text0:"How long have I known you, Danny?",text1:""},{row:321, start:0.0224884259259259, stop:0.0225231481481481, text0:"I was... I was 19, sir.",text1:""},{row:322, start:0.0225231481481481, stop:0.0225810185185185, text0:"And you survived.",text1:"A lot of boys didn't. Why is that?"},{row:323, start:0.0225925925925926, stop:0.0226273148148148, text0:"Just lucky, I guess.",text1:""},{row:324, start:0.022650462962963, stop:0.0227083333333333, text0:"No, you were a good soldier.",text1:"But I made you better, didn't I?"},{row:325, start:0.0227430555555556, stop:0.0227546296296296, text0:"Yes, sir. Yes, you did.",text1:""},{row:326, start:0.0227662037037037, stop:0.0228009259259259, text0:"And over the years,",text1:"didn't I protect you..."},{row:327, start:0.0228009259259259, stop:0.0228472222222222, text0:"...teach you about diamonds,",text1:"cut you in on the deals?"},{row:328, start:0.0228472222222222, stop:0.0228935185185185, text0:"- Some might call it that, yeah.",text1:"- But not you..."},{row:329, start:0.0229050925925926, stop:0.0229398148148148, text0:"...because you done with me now, eh?",text1:"On to bigger things."},{row:330, start:0.0229861111111111, stop:0.0230092592592593, text0:"I need a man that knows",text1:"his way around up there."},{row:331, start:0.0230092592592593, stop:0.0230555555555556, text0:"Unless, of course,",text1:"you wanna cut me in. Something pink?"},{row:332, start:0.0230671296296296, stop:0.0230902777777778, text0:"Sir...",text1:""},{row:333, start:0.0230902777777778, stop:0.0231134259259259, text0:"Our deal went bad,",text1:"and you owe me money."},{row:334, start:0.0231134259259259, stop:0.0231365740740741, text0:"I'll take a stone as payment.",text1:""},{row:335, start:0.0231365740740741, stop:0.0231828703703704, text0:"Look, do you think if I found a stone",text1:"like that, I'd still be on this continent?"},{row:336, start:0.0231828703703704, stop:0.0232291666666667, text0:"- Come on.",text1:"- Danny, give me your hand."},{row:337, start:0.0232638888888889, stop:0.0233101851851852, text0:"That's red earth. It's in our skin.",text1:""},{row:338, start:0.0233101851851852, stop:0.0233333333333333, text0:"The Shona say the color",text1:"comes from all the blood..."},{row:339, start:0.0233449074074074, stop:0.0233680555555556, text0:"...that's being spilled",text1:"fighting over the land."},{row:340, start:0.0233912037037037, stop:0.0234490740740741, text0:"This is home.",text1:"You'll never leave Africa."},{row:341, start:0.0234837962962963, stop:0.0235069444444444, text0:"If you say so, sir.",text1:""},{row:342, start:0.0236921296296296, stop:0.0237268518518518, text0:"So, what did you do, bury it?",text1:""},{row:343, start:0.02375, stop:0.0237847222222222, text0:"Yeah. Yeah, I would have",text1:"buried it, huh?"},{row:344, start:0.0237847222222222, stop:0.0238310185185185, text0:"- What are you talking about?",text1:"- What am I talking about? Come on, bru."},{row:345, start:0.0238425925925926, stop:0.0238541666666667, text0:"I'm late for work.",text1:""},{row:346, start:0.0238541666666667, stop:0.0238888888888889, text0:"Pink, huh? The biggest one",text1:"he'd ever seen."},{row:347, start:0.0238888888888889, stop:0.023912037037037, text0:"You were in the mining camps,",text1:"weren't you, bru?"},{row:348, start:0.023912037037037, stop:0.0239583333333333, text0:"Why else would that rebel with",text1:"the bandaged arm make up such a story?"},{row:349, start:0.0239583333333333, stop:0.0239814814814815, text0:"Go ask him.",text1:""},{row:350, start:0.0241435185185185, stop:0.0241666666666667, text0:"Thank you.",text1:""},{row:351, start:0.0242708333333333, stop:0.0243055555555556, text0:"Your mothers and fathers,",text1:"your brothers and sisters are dead."},{row:352, start:0.0243055555555556, stop:0.0243287037037037, text0:"- You are dead! You have been reborn.",text1:"- No!"},{row:353, start:0.0243287037037037, stop:0.0243634259259259, text0:"- We are your family now.",text1:"- No! No!"},{row:354, start:0.0244560185185185, stop:0.0244907407407407, text0:"Your parents are weak.",text1:""},{row:355, start:0.0244907407407407, stop:0.0245486111111111, text0:"They're the farmers.",text1:"They're the fishermen."},{row:356, start:0.0245486111111111, stop:0.0245949074074074, text0:"They've done nothing",text1:"but suck the blood from this country."},{row:357, start:0.0246064814814815, stop:0.0246643518518519, text0:"But you are the heroes",text1:"who will save this nation."},{row:358, start:0.0246643518518519, stop:0.0246875, text0:"You are not children anymore.",text1:""},{row:359, start:0.0246990740740741, stop:0.0247222222222222, text0:"You're men.",text1:""},{row:360, start:0.0247453703703704, stop:0.0247800925925926, text0:"No one has ever given you respect...",text1:""},{row:361, start:0.0247800925925926, stop:0.0248148148148148, text0:"...but with this in your hand,",text1:"they go fear you."},{row:362, start:0.0248263888888889, stop:0.024849537037037, text0:"If you do not get",text1:"the respect you deserve..."},{row:363, start:0.024849537037037, stop:0.0248842592592593, text0:"...you will take it",text1:"by shedding their blood."},{row:364, start:0.0248842592592593, stop:0.0249305555555556, text0:"- Repeat after me, shed their blood.",text1:"- Shed their blood."},{row:365, start:0.0249305555555556, stop:0.0249652777777778, text0:"- Louder! Shed their blood! Shed their blood!",text1:"- Shed their blood!"},{row:366, start:0.0249652777777778, stop:0.025, text0:"Make them hear you in Freetown.",text1:"Shed their blood! Shed their blood!"},{row:367, start:0.025, stop:0.0250347222222222, text0:"Shed their blood! Shed their blood!",text1:"Shed their blood!"},{row:368, start:0.0250347222222222, stop:0.0250694444444444, text0:"- Dia Vandy, stand up.",text1:"- Shed their blood! Shed their blood!"},{row:369, start:0.0250694444444444, stop:0.0251041666666667, text0:"- Shed their blood! Shed their blood!",text1:"- Put your hand on the gun."},{row:370, start:0.0251041666666667, stop:0.0251273148148148, text0:"Shed their blood!",text1:"Shed their blood!"},{row:371, start:0.0251273148148148, stop:0.025150462962963, text0:"Shed their blood!",text1:"Shed their blood!"},{row:372, start:0.025150462962963, stop:0.0251851851851852, text0:"- Shed their blood! Shed their blood!",text1:"- Now squeeze the trigger."},{row:373, start:0.0251851851851852, stop:0.0252430555555556, text0:"Come on, Dia. Pull the trigger.",text1:"Pull the trigger!"},{row:374, start:0.0256365740740741, stop:0.0256828703703704, text0:"I know, I know.",text1:"It's hard to be a man."},{row:375, start:0.0256944444444444, stop:0.0257291666666667, text0:"Sit up. Come.",text1:""},{row:376, start:0.0257291666666667, stop:0.025775462962963, text0:"But you are a soldier",text1:"of the revolution now."},{row:377, start:0.025775462962963, stop:0.0258101851851852, text0:"And I am your commander.",text1:""},{row:378, start:0.0258217592592593, stop:0.0258449074074074, text0:"Whatever you need...",text1:""},{row:379, start:0.0258449074074074, stop:0.0258796296296296, text0:"...guns, CDs, food...",text1:""},{row:380, start:0.0258796296296296, stop:0.0259259259259259, text0:"...come to me.",text1:"I will take care of you."},{row:381, start:0.0259606481481481, stop:0.0260416666666667, text0:"Today, you are promoted",text1:"to the rank of captain."},{row:382, start:0.026087962962963, stop:0.0261111111111111, text0:"Dia Vandy...",text1:""},{row:383, start:0.0261342592592593, stop:0.0261574074074074, text0:"...boss man.",text1:""},{row:384, start:0.0263541666666667, stop:0.0263773148148148, text0:"How's it, bru?",text1:""},{row:385, start:0.0263888888888889, stop:0.0264236111111111, text0:"- You got something for me, huh?",text1:"- Yeah."},{row:386, start:0.0264351851851852, stop:0.0264699074074074, text0:"- So, what's new, M'Ed?",text1:"- Fucking RUF."},{row:387, start:0.0264699074074074, stop:0.0265046296296296, text0:"Tried to hit the government building",text1:"across the street."},{row:388, start:0.0265046296296296, stop:0.0265509259259259, text0:"Shit, man, half the people",text1:"be soldiers today, RUF tomorrow."},{row:389, start:0.0265625, stop:0.0265972222222222, text0:"- Might be time to get your family out, huh?",text1:"- And go where?"},{row:390, start:0.0265972222222222, stop:0.0266435185185185, text0:"Just fire up the chopper",text1:"and fly away like you people?"},{row:391, start:0.0266435185185185, stop:0.0266666666666667, text0:"This my country, man.",text1:""},{row:392, start:0.0266666666666667, stop:0.026712962962963, text0:"We here long before you came,",text1:"long after you gone."},{row:393, start:0.0268171296296296, stop:0.0268518518518518, text0:"- Hi. I'm Maddy Bowen.",text1:"- How about you dance with me?"},{row:394, start:0.0268518518518518, stop:0.0268981481481481, text0:"- No, I don't dance.",text1:"- Neither do I. Come on."},{row:395, start:0.0269560185185185, stop:0.0269791666666667, text0:"- So, Danny...",text1:"- Archer."},{row:396, start:0.0269791666666667, stop:0.0269907407407407, text0:"Archer.",text1:""},{row:397, start:0.0270023148148148, stop:0.0270486111111111, text0:"When did you first start",text1:"moving stones? Was it Angola?"},{row:398, start:0.0270717592592593, stop:0.0271180555555556, text0:"Then you did the whole",text1:"mercenary thing. Was that fun?"},{row:399, start:0.0271412037037037, stop:0.0271875, text0:"So, what are you?",text1:"Nihilist? Opportunist?"},{row:400, start:0.0271875, stop:0.0272106481481481, text0:"Maybe I wasn't breast-fed",text1:"as a child, huh?"},{row:401, start:0.0272337962962963, stop:0.0272685185185185, text0:"You think I haven't met",text1:"people like you before?"},{row:402, start:0.0272685185185185, stop:0.0273032407407407, text0:"- I think you get off on people like me.",text1:"- You think?"},{row:403, start:0.027337962962963, stop:0.0273611111111111, text0:"When did you first start",text1:"working for Van De Kaap?"},{row:404, start:0.0273726851851852, stop:0.0273958333333333, text0:"Christ, you never stop, do you?",text1:""},{row:405, start:0.0274074074074074, stop:0.0274421296296296, text0:"No, I never stop.",text1:""},{row:406, start:0.0274537037037037, stop:0.0274652777777778, text0:"Do you want me to stop?",text1:""},{row:407, start:0.0274884259259259, stop:0.0275347222222222, text0:"Why don't we go back to your place,",text1:"see what's in the minibar, huh?"},{row:408, start:0.0275347222222222, stop:0.0275578703703704, text0:"I'm a print journalist. I drank it.",text1:""},{row:409, start:0.0275694444444444, stop:0.0276041666666667, text0:"Is it possible that you don't care",text1:"how many people die..."},{row:410, start:0.0276041666666667, stop:0.0276273148148148, text0:"...because of the deals you do?",text1:""},{row:411, start:0.0276273148148148, stop:0.0276736111111111, text0:"People here kill each other as a way of life.",text1:"Always been like that."},{row:412, start:0.0276736111111111, stop:0.0277083333333333, text0:"So you can just watch it",text1:"and go about your day?"},{row:413, start:0.0277199074074074, stop:0.0277430555555556, text0:"Maybe we should all just",text1:"write about it, then."},{row:414, start:0.0277430555555556, stop:0.0277777777777778, text0:"Look, thanks for the dance, huh?",text1:""},{row:415, start:0.0278009259259259, stop:0.0278356481481482, text0:"- And a shot too, huh, M'Ed?",text1:"- Sure."},{row:416, start:0.0278819444444444, stop:0.0278935185185185, text0:"You're a hard case, huh?",text1:""},{row:417, start:0.0278935185185185, stop:0.0279282407407407, text0:"Tell me something,",text1:"how long you been here in Africa, huh?"},{row:418, start:0.0279398148148148, stop:0.027974537037037, text0:"Three months. And before that,",text1:"Afghanistan and Bosnia."},{row:419, start:0.027974537037037, stop:0.0279976851851852, text0:"You come here",text1:"with your laptop computer..."},{row:420, start:0.0279976851851852, stop:0.0280439814814815, text0:"...and your malaria medicine",text1:"and your little bottles of hand sanitizer."},{row:421, start:0.0280555555555556, stop:0.0280902777777778, text0:"Think you're gonna change",text1:"the outcome, huh?"},{row:422, start:0.0280902777777778, stop:0.028125, text0:"Let me tell you something.",text1:"You sell blood diamonds too."},{row:423, start:0.028125, stop:0.0281481481481481, text0:"- Really?",text1:"- Yeah."},{row:424, start:0.0281481481481481, stop:0.0281597222222222, text0:"Tell me, how is that?",text1:""},{row:425, start:0.0281597222222222, stop:0.0281828703703704, text0:"Who do you think buys",text1:"the stones that I bring out?"},{row:426, start:0.0281828703703704, stop:0.0282291666666667, text0:"Dreamy American girls who all want",text1:"a storybook wedding and a big, shiny rock..."},{row:427, start:0.0282291666666667, stop:0.028275462962963, text0:"...like the ones in the advertisements",text1:"of your politically-correct magazines."},{row:428, start:0.028275462962963, stop:0.0283101851851852, text0:"So, please, don't come here",text1:"and make judgments on me, all right?"},{row:429, start:0.0283217592592593, stop:0.0283796296296296, text0:"I provide a service. The world wants what",text1:"we have, and they want it cheap."},{row:430, start:0.0283796296296296, stop:0.0284143518518518, text0:"We're in business together.",text1:"Get over yourself, darling."},{row:431, start:0.0284259259259259, stop:0.0284722222222222, text0:"Okay, just to clarify, not all American girls",text1:"want a storybook wedding..."},{row:432, start:0.0284722222222222, stop:0.0285069444444444, text0:"...just like not all Africans kill each other",text1:"as a way of life."},{row:433, start:0.0285069444444444, stop:0.0285300925925926, text0:"And yeah, it's a shit, shit world,",text1:"but you know what?"},{row:434, start:0.0285300925925926, stop:0.0285763888888889, text0:"Good things are done every day,",text1:"just, apparently, not by you."},{row:435, start:0.028599537037037, stop:0.0286342592592593, text0:"This place is about to explode.",text1:""},{row:436, start:0.0286342592592593, stop:0.0286574074074074, text0:"I wanna spend my time",text1:"with someone else."},{row:437, start:0.0287847222222222, stop:0.0288194444444444, text0:"You are listening to BBC World Service.",text1:""},{row:438, start:0.0288310185185185, stop:0.0288657407407407, text0:"In Sierra Leone, Revolutionary United Front",text1:"forces have been sighted..."},{row:439, start:0.0288657407407407, stop:0.028900462962963, text0:"...within 10 kilometers",text1:"of the capital city of Freetown."},{row:440, start:0.028900462962963, stop:0.0289351851851852, text0:"The minister of the interior",text1:"has expressed confidence..."},{row:441, start:0.0289351851851852, stop:0.0289699074074074, text0:"...that the government troops",text1:"can repel the attack."},{row:442, start:0.0289699074074074, stop:0.0290046296296296, text0:"All foreign nationals are being advised",text1:"to leave the country."},{row:443, start:0.0290046296296296, stop:0.0290277777777778, text0:"There are also unconfirmed reports...",text1:""},{row:444, start:0.0290277777777778, stop:0.0290856481481481, text0:"...that atrocities are being committed by",text1:"both sides in the rapidly worsening situation."},{row:445, start:0.0290856481481481, stop:0.0291319444444444, text0:"The United Nations has described",text1:"the refugee situation as critical."},{row:446, start:0.0292592592592593, stop:0.0292939814814815, text0:"Don't tell me you're gonna try",text1:"and sell it yourself."},{row:447, start:0.0292939814814815, stop:0.0293171296296296, text0:"To who?",text1:"And for what price, my friend?"},{row:448, start:0.0293287037037037, stop:0.0293518518518519, text0:"You need my help,",text1:"whether you like it or not."},{row:449, start:0.0293518518518519, stop:0.0293865740740741, text0:"- What are you talking about?",text1:"- Here, let me help you with that."},{row:450, start:0.0293865740740741, stop:0.0294328703703704, text0:"You hear that? You hear that?",text1:"They came into the city overnight."},{row:451, start:0.0294328703703704, stop:0.0294560185185185, text0:"It's started.",text1:"What are you gonna do now, huh?"},{row:452, start:0.0294560185185185, stop:0.0295138888888889, text0:"Listen, the right stone can buy anything.",text1:"Information, safety, even freedom."},{row:453, start:0.0295138888888889, stop:0.029537037037037, text0:"But a big stone does not stay secret",text1:"for very long."},{row:454, start:0.0295486111111111, stop:0.0295833333333333, text0:"The moment you tell anyone about it,",text1:"your life is absolutely worthless."},{row:455, start:0.0295833333333333, stop:0.0296180555555556, text0:"The only reason you're alive is",text1:"you haven't told anyone where it is."},{row:456, start:0.0296180555555556, stop:0.0296412037037037, text0:"Am I right? Am I?",text1:""},{row:457, start:0.0296412037037037, stop:0.0296643518518519, text0:"Who do you think got you",text1:"out of jail, huh?"},{row:458, start:0.0296643518518519, stop:0.0296990740740741, text0:"- That makes us partners.",text1:"- I am not your partner!"},{row:459, start:0.0296990740740741, stop:0.0297337962962963, text0:"What if I helped you find your family?",text1:""},{row:460, start:0.0297337962962963, stop:0.0297569444444444, text0:"What do you know of my family?",text1:""},{row:461, start:0.0297569444444444, stop:0.0297916666666667, text0:"The relief agencies are useless,",text1:"the hospitals are overwhelmed."},{row:462, start:0.0297916666666667, stop:0.0298263888888889, text0:"- There are other ways, bru.",text1:"- Liar."},{row:463, start:0.0298263888888889, stop:0.0298611111111111, text0:"Look at me. I know people, huh?",text1:"White people."},{row:464, start:0.0298611111111111, stop:0.0298958333333333, text0:"Without me, you're just another",text1:"black man in Africa, all right?"},{row:465, start:0.0299421296296296, stop:0.0299537037037037, text0:"Oh, shit.",text1:""},{row:466, start:0.0299768518518519, stop:0.03, text0:"We got no time.",text1:"What's it gonna be?"},{row:467, start:0.03, stop:0.0300231481481481, text0:"You would say anything.",text1:"How can I trust you?"},{row:468, start:0.0300231481481481, stop:0.0300578703703704, text0:"I don't give a fuck about you, all right?",text1:"That diamond could be priceless!"},{row:469, start:0.0300694444444444, stop:0.0301157407407407, text0:"We split it, and you get your family.",text1:"What's it gonna be, yes or no? Yes or no?"},{row:470, start:0.0301157407407407, stop:0.030150462962963, text0:"- Where will we go? Where do we go?",text1:"- Come on, follow me!"},{row:471, start:0.0305092592592593, stop:0.0305324074074074, text0:"Stay low!",text1:""},{row:472, start:0.0305555555555556, stop:0.0305671296296296, text0:"Get your ass over here!",text1:""},{row:473, start:0.0306944444444444, stop:0.0307060185185185, text0:"Wait!",text1:""},{row:474, start:0.0307060185185185, stop:0.0307291666666667, text0:"Now!",text1:""},{row:475, start:0.030787037037037, stop:0.0308101851851852, text0:"Come on, come on.",text1:""},{row:476, start:0.0308564814814815, stop:0.0308680555555556, text0:"Come on!",text1:""},{row:477, start:0.0310532407407407, stop:0.0310763888888889, text0:"Come on.",text1:""},{row:478, start:0.0310763888888889, stop:0.031087962962963, text0:"Fuck!",text1:""},{row:479, start:0.0312037037037037, stop:0.0312152777777778, text0:"No, this way.",text1:""},{row:480, start:0.0312268518518519, stop:0.0312384259259259, text0:"Fire!",text1:""},{row:481, start:0.0314236111111111, stop:0.0314467592592593, text0:"Fuck! Go, go, go!",text1:""},{row:482, start:0.0314467592592593, stop:0.0314699074074074, text0:"Go!",text1:""},{row:483, start:0.0315856481481481, stop:0.0316087962962963, text0:"Shit!",text1:""},{row:484, start:0.0316087962962963, stop:0.0316203703703704, text0:"No! No!",text1:""},{row:485, start:0.031724537037037, stop:0.0317476851851852, text0:"This way.",text1:""},{row:486, start:0.0319791666666667, stop:0.0320023148148148, text0:"This way. Stay low.",text1:""},{row:487, start:0.0321296296296296, stop:0.0321527777777778, text0:"Come on. Go, go, go. Come on.",text1:""},{row:488, start:0.0322337962962963, stop:0.0322569444444444, text0:"Come on!",text1:""},{row:489, start:0.0323148148148148, stop:0.032349537037037, text0:"It's okay. It's okay, it's okay.",text1:""},{row:490, start:0.0326388888888889, stop:0.032662037037037, text0:"Fire!",text1:""},{row:491, start:0.0338425925925926, stop:0.0338888888888889, text0:"- Is there another way?",text1:"- That bridge is the only way across, huh?"},{row:492, start:0.0338888888888889, stop:0.033912037037037, text0:"So, what do we do?",text1:""},{row:493, start:0.0340393518518519, stop:0.0340972222222222, text0:"- You are RUF, huh? And I am your prisoner.",text1:"- What do you mean?"},{row:494, start:0.0340972222222222, stop:0.0341319444444444, text0:"You wanna see your son again, don't you?",text1:"You stay behind me."},{row:495, start:0.0341435185185185, stop:0.0341550925925926, text0:"Wait, wait. Wait.",text1:""},{row:496, start:0.0341898148148148, stop:0.034224537037037, text0:"Bring that pink chicken skin",text1:"over here, huh?"},{row:497, start:0.034224537037037, stop:0.0342592592592593, text0:"Now let me shoot that pink ass, boy.",text1:""},{row:498, start:0.0346643518518518, stop:0.0346875, text0:"Where are we going?",text1:""},{row:499, start:0.0346990740740741, stop:0.0347337962962963, text0:"Lungi's under government control.",text1:""},{row:500, start:0.0347337962962963, stop:0.0347800925925926, text0:"I'm gonna try to locate a sat phone",text1:"to get in contact with my pilot, all right?"},{row:501, start:0.0347916666666667, stop:0.0348148148148148, text0:"I have agreed to nothing.",text1:""},{row:502, start:0.0348263888888889, stop:0.034849537037037, text0:"You're in a bit of a conundrum there,",text1:"my friend."},{row:503, start:0.034849537037037, stop:0.0349074074074074, text0:"You know what that means?",text1:"It means you have no choice."},{row:504, start:0.0350694444444444, stop:0.0351041666666667, text0:"So you're a fisherman, huh?",text1:""},{row:505, start:0.0351041666666667, stop:0.035150462962963, text0:"- What do you catch mostly?",text1:"- Fish."},{row:506, start:0.0352430555555556, stop:0.0352662037037037, text0:"Come on.",text1:""},{row:507, start:0.0354050925925926, stop:0.0354166666666667, text0:"- Hello.",text1:"- Back away from the fence."},{row:508, start:0.0354166666666667, stop:0.0354513888888889, text0:"Listen, I'm from",text1:"the Cape Herald, all right?"},{row:509, start:0.0354513888888889, stop:0.0354861111111111, text0:"My wallet, my credentials,",text1:"everything got left in Freetown."},{row:510, start:0.0354861111111111, stop:0.0355208333333333, text0:"We barely got out with our lives.",text1:"Can we just get through?"},{row:511, start:0.0355208333333333, stop:0.0355555555555556, text0:"Back away from the fence.",text1:""},{row:512, start:0.0356018518518518, stop:0.035625, text0:"But you are not a journalist.",text1:""},{row:513, start:0.035625, stop:0.0356365740740741, text0:"I'm trying to get us to Kono.",text1:""},{row:514, start:0.0356481481481482, stop:0.0356712962962963, text0:"- Do you understand that, huh?",text1:"- Getting B-roll. Wait."},{row:515, start:0.0356712962962963, stop:0.0357060185185185, text0:"Listen, I'm coming back.",text1:"I'm coming back just now."},{row:516, start:0.0357060185185185, stop:0.0357407407407407, text0:"How's it, my bru?",text1:"Have you got an extra smoke?"},{row:517, start:0.0357407407407407, stop:0.0357638888888889, text0:"- Here.",text1:"- Oh, cheers."},{row:518, start:0.0357638888888889, stop:0.0357986111111111, text0:"I'm looking for someone, an American",text1:"by the name of Maddy Bowen."},{row:519, start:0.0357986111111111, stop:0.0358333333333333, text0:"- If you saw her, you would not forget it.",text1:"- Yeah, I know her."},{row:520, start:0.0358333333333333, stop:0.0358449074074074, text0:"- Is it?",text1:"- Yeah."},{row:521, start:0.0358449074074074, stop:0.0358912037037037, text0:"Can you tell her Danny Archer's looking",text1:"for her? I've got the story she wants."},{row:522, start:0.0358912037037037, stop:0.0359259259259259, text0:"Okay, but I'm going out for a bit.",text1:"If I see her before, I'll tell her."},{row:523, start:0.0359375, stop:0.0359606481481482, text0:"Magic. Listen, we'll be",text1:"right down the road, huh?"},{row:524, start:0.0359606481481482, stop:0.0359953703703704, text0:"- Sweet. Cheers, bru.",text1:"- Cheers."},{row:525, start:0.0360069444444444, stop:0.0360532407407407, text0:"It is as I said.",text1:"You will say anything."},{row:526, start:0.0361226851851852, stop:0.0361458333333333, text0:"- You don't mind waiting?",text1:"- No worries."},{row:527, start:0.0361458333333333, stop:0.0361921296296296, text0:"- Thank you so much. I'll just be a sec.",text1:"- All right, Maddy."},{row:528, start:0.0362268518518519, stop:0.03625, text0:"Hi.",text1:""},{row:529, start:0.03625, stop:0.0362731481481481, text0:"You got out.",text1:""},{row:530, start:0.0362731481481481, stop:0.0363078703703704, text0:"Yes. Hello again.",text1:""},{row:531, start:0.0363078703703704, stop:0.0363310185185185, text0:"- This is Solomon Vandy.",text1:"- Hi."},{row:532, start:0.0363310185185185, stop:0.0363541666666667, text0:"- Hello.",text1:"- Nice to meet you."},{row:533, start:0.0363541666666667, stop:0.036400462962963, text0:"So I'm on deadline. I've been waiting",text1:"four hours to use the phone."},{row:534, start:0.036400462962963, stop:0.0364467592592593, text0:"- I've got someone holding my place in line.",text1:"- I understand. Come with me, huh?"},{row:535, start:0.0364583333333333, stop:0.0364814814814815, text0:"All right. Here it is.",text1:""},{row:536, start:0.0364814814814815, stop:0.0365162037037037, text0:"- There's a company called Tiara Diamonds.",text1:"- Right?"},{row:537, start:0.0365162037037037, stop:0.0365393518518519, text0:"They get the stones",text1:"that I smuggle into Liberia."},{row:538, start:0.0365509259259259, stop:0.0365740740740741, text0:"Now, Van De Kaap",text1:"has no visible ties to Tiara..."},{row:539, start:0.0365740740740741, stop:0.0366087962962963, text0:"...but through a series of cross holdings",text1:"and offshore bank accounts..."},{row:540, start:0.0366203703703704, stop:0.0366435185185185, text0:"- ...they actually own it.",text1:"- You can prove this?"},{row:541, start:0.0366550925925926, stop:0.0367013888888889, text0:"- If you help me out, yeah.",text1:"- Right, here it comes."},{row:542, start:0.0367013888888889, stop:0.036724537037037, text0:"Before you say anything,",text1:"look at that man. You see him?"},{row:543, start:0.0367361111111111, stop:0.0367824074074074, text0:"His entire village was burned down.",text1:"His wife and children, they got away."},{row:544, start:0.0367939814814815, stop:0.0368287037037037, text0:"He's been working in the mining camps,",text1:"trying to get his family back."},{row:545, start:0.0368287037037037, stop:0.0368518518518519, text0:"He cannot get through the red tape.",text1:""},{row:546, start:0.0368518518518519, stop:0.036875, text0:"All I'm asking is this,",text1:"that you help him, huh?"},{row:547, start:0.0368865740740741, stop:0.0369097222222222, text0:"And how do you know this man?",text1:""},{row:548, start:0.0369097222222222, stop:0.0369560185185185, text0:"- I know him. I know him.",text1:"- Oh, come on, Archer."},{row:549, start:0.0369675925925926, stop:0.0369791666666667, text0:"You're using him.",text1:""},{row:550, start:0.0370023148148148, stop:0.0370486111111111, text0:"I'm using him, and you are using me,",text1:"and this is how it works, isn't it?"},{row:551, start:0.0370486111111111, stop:0.0370833333333333, text0:"Come on. Look, you've got access",text1:"to U.N. databases."},{row:552, start:0.0370833333333333, stop:0.0371064814814815, text0:"- You can track his family for him.",text1:"- Why?"},{row:553, start:0.0371064814814815, stop:0.0371412037037037, text0:"This whole country's at war.",text1:"Why should I help just one person?"},{row:554, start:0.0372106481481482, stop:0.0372337962962963, text0:"I can't believe I just said that.",text1:""},{row:555, start:0.0372685185185185, stop:0.0373032407407407, text0:"My family's here, in Guinea?",text1:""},{row:556, start:0.0373032407407407, stop:0.0373263888888889, text0:"Yes, according to the manifest.",text1:""},{row:557, start:0.037337962962963, stop:0.0373611111111111, text0:"I do not know how to thank you.",text1:""},{row:558, start:0.037662037037037, stop:0.0376967592592593, text0:"Thank you. Thank you.",text1:""},{row:559, start:0.0378587962962963, stop:0.0378819444444444, text0:"This is what a million people looks like.",text1:""},{row:560, start:0.0379282407407407, stop:0.037974537037037, text0:"At the moment, the second largest",text1:"refugee camp in Africa."},{row:561, start:0.037974537037037, stop:0.0380439814814815, text0:"Might catch a minute of this on CNN,",text1:"somewhere between Sports and Weather."},{row:562, start:0.0381481481481481, stop:0.0381712962962963, text0:"- Hello.",text1:"- Yeah, hello?"},{row:563, start:0.0381712962962963, stop:0.0381944444444444, text0:"I've spoken with Colonel Coedoo...",text1:""},{row:564, start:0.0382060185185185, stop:0.0382407407407407, text0:"...and highlighted the names",text1:"we're looking for."},{row:565, start:0.0382407407407407, stop:0.0382523148148148, text0:"- Move away.",text1:"- What him doing?"},{row:566, start:0.0382523148148148, stop:0.0382638888888889, text0:"Okay, I'm gonna check for you.",text1:""},{row:567, start:0.038275462962963, stop:0.0382986111111111, text0:"- Wait me there.",text1:"- Okay, thank you."},{row:568, start:0.0382986111111111, stop:0.0383217592592593, text0:"We are here to help you.",text1:""},{row:569, start:0.0383217592592593, stop:0.0383449074074074, text0:"This woman says she has",text1:"spoken to Coedoo."},{row:570, start:0.0383449074074074, stop:0.0383912037037037, text0:"An entire country made homeless.",text1:""},{row:571, start:0.0384259259259259, stop:0.0384606481481481, text0:"Two multiplied by three",text1:"is equal to six."},{row:572, start:0.0384606481481481, stop:0.0385069444444444, text0:"Two multiplied by four",text1:"is equal to eight."},{row:573, start:0.0385069444444444, stop:0.0385416666666667, text0:"Two multiplied by five",text1:"is equal to 10."},{row:574, start:0.0385532407407407, stop:0.038587962962963, text0:"Two multiplied by six",text1:"is equal to 12."},{row:575, start:0.0390277777777778, stop:0.0390393518518519, text0:"Jassie.",text1:""},{row:576, start:0.0390509259259259, stop:0.0390625, text0:"Jassie!",text1:""},{row:577, start:0.0390740740740741, stop:0.0390972222222222, text0:"Jassie!",text1:""},{row:578, start:0.0390972222222222, stop:0.0391087962962963, text0:"- Jassie.",text1:"- Solo."},{row:579, start:0.0391087962962963, stop:0.0391319444444444, text0:"- Jassie.",text1:"- Solo."},{row:580, start:0.0391319444444444, stop:0.0391550925925926, text0:"Jassie. Jassie.",text1:""},{row:581, start:0.0391782407407407, stop:0.0392013888888889, text0:"- Solomon.",text1:"- Jassie."},{row:582, start:0.0393055555555556, stop:0.0393171296296296, text0:"- Solomon.",text1:"- Jassie."},{row:583, start:0.0393402777777778, stop:0.0393634259259259, text0:"Dia. Dia.",text1:""},{row:584, start:0.039375, stop:0.0393865740740741, text0:"Where is Dia?",text1:""},{row:585, start:0.0394328703703704, stop:0.0394444444444444, text0:"Jassie!",text1:""},{row:586, start:0.0395717592592593, stop:0.0395949074074074, text0:"They took him.",text1:""},{row:587, start:0.0396412037037037, stop:0.0396643518518519, text0:"There are only two children",text1:"on the manifest."},{row:588, start:0.0396643518518519, stop:0.0396990740740741, text0:"- Where is my son?",text1:"- Oh, Christ."},{row:589, start:0.0396990740740741, stop:0.0397222222222222, text0:"Where is my son?",text1:""},{row:590, start:0.0397222222222222, stop:0.0397453703703704, text0:"- Away from the fence.",text1:"- Where is my son?"},{row:591, start:0.0397453703703704, stop:0.0397569444444444, text0:"Move away from the fence.",text1:""},{row:592, start:0.0397685185185185, stop:0.0398032407407407, text0:"- Let them out! Let them out!",text1:"- Keep away from the fence."},{row:593, start:0.0398148148148148, stop:0.0398263888888889, text0:"Let them out!",text1:""},{row:594, start:0.0398726851851852, stop:0.0398842592592593, text0:"Away from the fence.",text1:""},{row:595, start:0.0399074074074074, stop:0.0399189814814815, text0:"- They'll shoot you!",text1:"- Jassie!"},{row:596, start:0.0399305555555556, stop:0.0399537037037037, text0:"No! Solo!",text1:""},{row:597, start:0.0399537037037037, stop:0.0399768518518519, text0:"Do you want your wife and child",text1:"to watch you die?"},{row:598, start:0.0399884259259259, stop:0.0400231481481481, text0:"Solo! Solo!",text1:""},{row:599, start:0.0401273148148148, stop:0.0401851851851852, text0:"They said they're concerned that",text1:"some of the refugees might be rebels..."},{row:600, start:0.0401967592592593, stop:0.0402314814814815, text0:"...so they refused to release them",text1:"until there's a cease-fire."},{row:601, start:0.0402546296296296, stop:0.0402777777777778, text0:"I'm so sorry, Solomon.",text1:""},{row:602, start:0.0402893518518518, stop:0.0403125, text0:"What you want, Mr. Archer,",text1:"is beyond those hills..."},{row:603, start:0.0403240740740741, stop:0.0403472222222222, text0:"...where I buried it.",text1:""},{row:604, start:0.0406134259259259, stop:0.0406712962962963, text0:"You say the mine is on the Sewa River.",text1:"Where exactly?"},{row:605, start:0.0406828703703704, stop:0.0407060185185185, text0:"Solomon.",text1:""},{row:606, start:0.0407175925925926, stop:0.0407407407407407, text0:"I do not need a map.",text1:""},{row:607, start:0.0407407407407407, stop:0.040787037037037, text0:"Yeah, well, I do.",text1:"Look, we are down here, huh?"},{row:608, start:0.040787037037037, stop:0.0408101851851852, text0:"The press convoy",text1:"is on its way to Kono."},{row:609, start:0.0408101851851852, stop:0.0408564814814815, text0:"You say the diamond mine is up here.",text1:"Where did you bury it?"},{row:610, start:0.0408912037037037, stop:0.0409259259259259, text0:"When we get to Kono,",text1:"we're gonna have to walk, all right?"},{row:611, start:0.0409259259259259, stop:0.0409606481481481, text0:"You damn well better be able to find it.",text1:""},{row:612, start:0.0409837962962963, stop:0.0410300925925926, text0:"My son is a very good student.",text1:""},{row:613, start:0.0410416666666667, stop:0.0410763888888889, text0:"He walks five kilometers",text1:"to the Sister School..."},{row:614, start:0.0410763888888889, stop:0.0411111111111111, text0:"...every morning to study English.",text1:""},{row:615, start:0.0411342592592593, stop:0.0411689814814815, text0:"He's going to be a doctor one day.",text1:""},{row:616, start:0.0414583333333333, stop:0.0415046296296296, text0:"'In the 90-degree heat,",text1:"Solomon Vandy drops to his knees..."},{row:617, start:0.0415046296296296, stop:0.0415393518518519, text0:"...rattling the wire",text1:"with his powerful hands.'"},{row:618, start:0.0415856481481481, stop:0.0416203703703704, text0:"- What?",text1:"- Nothing."},{row:619, start:0.0416435185185185, stop:0.0416666666666667, text0:"Do you think I'm exploiting his grief?",text1:""},{row:620, start:0.0416898148148148, stop:0.041712962962963, text0:"You're right. It's shit.",text1:""},{row:621, start:0.041712962962963, stop:0.0417476851851852, text0:"It's like one of those infomercials.",text1:""},{row:622, start:0.0417476851851852, stop:0.0418055555555556, text0:"You know, the little black babies",text1:"with swollen bellies and flies in their eyes."},{row:623, start:0.0418055555555556, stop:0.0418518518518519, text0:"So here I've got dead mothers,",text1:"I've got severed limbs, but it's nothing new."},{row:624, start:0.0418518518518519, stop:0.0418865740740741, text0:"It might be enough",text1:"to make people cry if they read it..."},{row:625, start:0.0418865740740741, stop:0.0419097222222222, text0:"...maybe even write a check...",text1:""},{row:626, start:0.0419097222222222, stop:0.0419328703703704, text0:"...but it's not gonna be enough",text1:"to make it stop."},{row:627, start:0.0419328703703704, stop:0.0419907407407407, text0:"I am sick of writing about victims,",text1:"but it's all I can fucking do."},{row:628, start:0.0419907407407407, stop:0.0420138888888889, text0:"Because I need facts.",text1:""},{row:629, start:0.0420138888888889, stop:0.0420601851851852, text0:"I need names. I need dates.",text1:"I need pictures. I need bank accounts."},{row:630, start:0.0420833333333333, stop:0.0421180555555556, text0:"People back home",text1:"wouldn't buy a ring..."},{row:631, start:0.0421180555555556, stop:0.0421412037037037, text0:"...if they knew it cost",text1:"someone else their hand."},{row:632, start:0.0421527777777778, stop:0.0421759259259259, text0:"But I can't write that story...",text1:""},{row:633, start:0.0421759259259259, stop:0.0422222222222222, text0:"...until I get facts",text1:"that can be verified..."},{row:634, start:0.0422222222222222, stop:0.0422800925925926, text0:"...which is to say, until I find someone",text1:"who will go on record."},{row:635, start:0.0422916666666667, stop:0.0423148148148148, text0:"So if that is not you...",text1:""},{row:636, start:0.0423148148148148, stop:0.0423611111111111, text0:"...and you're not really gonna help me,",text1:"and we're not really gonna screw..."},{row:637, start:0.0423611111111111, stop:0.0424074074074074, text0:"...then why don't you get the fuck",text1:"out of my face..."},{row:638, start:0.0424074074074074, stop:0.0424305555555556, text0:"...and let me do my work?",text1:""},{row:639, start:0.0425231481481482, stop:0.0425694444444444, text0:"Do you know that Solomon thinks",text1:"his son's gonna be a doctor one day?"},{row:640, start:0.0426041666666667, stop:0.0426388888888889, text0:"Maybe his baby dies in that camp.",text1:"Maybe his daughter gets raped."},{row:641, start:0.0426388888888889, stop:0.042662037037037, text0:"Who knows, maybe both.",text1:""},{row:642, start:0.0426851851851852, stop:0.0427083333333333, text0:"Do you realize that",text1:"that diamond..."},{row:643, start:0.0427083333333333, stop:0.0427430555555556, text0:"...is the only chance he has",text1:"of getting his family out?"},{row:644, start:0.0427430555555556, stop:0.0427893518518518, text0:"- You don't give a rat's ass about his family.",text1:"- Do you understand that?"},{row:645, start:0.0428587962962963, stop:0.0428819444444444, text0:"Listen, I need to get to Kono, all right?",text1:""},{row:646, start:0.0428819444444444, stop:0.0429050925925926, text0:"The only way",text1:"is with the press convoy."},{row:647, start:0.0429050925925926, stop:0.0429398148148148, text0:"I'm a journalist,",text1:"and he's my cameraman, all right?"},{row:648, start:0.0429398148148148, stop:0.0429513888888889, text0:"- No.",text1:"- Please, Maddy."},{row:649, start:0.042962962962963, stop:0.042974537037037, text0:"- Please.",text1:"- No!"},{row:650, start:0.0431365740740741, stop:0.0431597222222222, text0:"After I smuggle the stones",text1:"across the border..."},{row:651, start:0.0431712962962963, stop:0.0432060185185185, text0:"...local buyers get them",text1:"to a middleman in Monrovia."},{row:652, start:0.0432523148148148, stop:0.0432638888888889, text0:"Go on.",text1:""},{row:653, start:0.043275462962963, stop:0.0433333333333333, text0:"He pays off Customs and then certifies",text1:"that the diamonds were mined in Liberia."},{row:654, start:0.0433333333333333, stop:0.0433680555555556, text0:"That way, they can be legally exported.",text1:""},{row:655, start:0.0433680555555556, stop:0.0434143518518518, text0:"Now, once they've reached",text1:"the buyers in Antwerp..."},{row:656, start:0.0434143518518518, stop:0.0434375, text0:"...diamonds are brought",text1:"to the sorting tables..."},{row:657, start:0.0434375, stop:0.0434606481481482, text0:"...no more questions are asked.",text1:""},{row:658, start:0.0434722222222222, stop:0.0434953703703704, text0:"By the time they get to India...",text1:""},{row:659, start:0.0434953703703704, stop:0.0435416666666667, text0:"...the dirty stones are mixed with",text1:"the clean stones from all over the world..."},{row:660, start:0.0435416666666667, stop:0.0435648148148148, text0:"...and then they become",text1:"like any other diamonds."},{row:661, start:0.0435648148148148, stop:0.043599537037037, text0:"And Van De Kaap",text1:"knows about all this?"},{row:662, start:0.0436111111111111, stop:0.0436226851851852, text0:"Yeah.",text1:""},{row:663, start:0.0436226851851852, stop:0.0436574074074074, text0:"When I get to London,",text1:"I meet with Simmons."},{row:664, start:0.0436689814814815, stop:0.0437037037037037, text0:"Supply and demand. You control the supply,",text1:"and you keep the demand high."},{row:665, start:0.0437037037037037, stop:0.0437268518518519, text0:"- Agreed.",text1:"- Good."},{row:666, start:0.0437268518518519, stop:0.04375, text0:"Now, there's an underground vault...",text1:""},{row:667, start:0.04375, stop:0.0437962962962963, text0:"...where they put all the stones",text1:"they buy up to keep off the market..."},{row:668, start:0.0437962962962963, stop:0.0438194444444444, text0:"...so they can keep the price high.",text1:""},{row:669, start:0.0438310185185185, stop:0.0438657407407407, text0:"If rebels wanna flood the market",text1:"with a billion dollars' worth of rough..."},{row:670, start:0.0438657407407407, stop:0.043900462962963, text0:"...a company like Van De Kaap,",text1:"who says that they're rare..."},{row:671, start:0.043900462962963, stop:0.0439236111111111, text0:"...can't afford to let that happen.",text1:""},{row:672, start:0.0439236111111111, stop:0.0439583333333333, text0:"Especially when they're telling",text1:"some poor sod..."},{row:673, start:0.0439583333333333, stop:0.0439814814814815, text0:"...he's supposed to shell out",text1:"three months' salary..."},{row:674, start:0.0439814814814815, stop:0.0440046296296296, text0:"...for an engagement ring.",text1:""},{row:675, start:0.0440046296296296, stop:0.0440393518518519, text0:"Now, technically speaking,",text1:"they're not financing the war..."},{row:676, start:0.0440393518518519, stop:0.0440740740740741, text0:"...but creating a situation",text1:"where it pays to keep it going."},{row:677, start:0.0440740740740741, stop:0.0441087962962963, text0:"- You understand?",text1:"- Yes. And where's my proof?"},{row:678, start:0.0441898148148148, stop:0.0442361111111111, text0:"Names, dates and numbered accounts.",text1:""},{row:679, start:0.0442592592592593, stop:0.0442939814814815, text0:"You publish one word of this story",text1:"before I give them the stone..."},{row:680, start:0.0442939814814815, stop:0.0443171296296296, text0:"...and I'm dead, huh?",text1:""},{row:681, start:0.0443171296296296, stop:0.044375, text0:"After I give it to them,",text1:"then... I leave this continent forever."},{row:682, start:0.0443981481481482, stop:0.0444212962962963, text0:"If you don't come out",text1:"with the diamond?"},{row:683, start:0.0444212962962963, stop:0.0444444444444444, text0:"Then write whatever you want.",text1:""},{row:684, start:0.0444560185185185, stop:0.0444791666666667, text0:"I'm already dead.",text1:""},{row:685, start:0.0445601851851852, stop:0.0446064814814815, text0:"Now, remember. All you say is,",text1:"'I am the cameraman,' all right?"},{row:686, start:0.0446064814814815, stop:0.0446296296296296, text0:"Listen, I cannot do this.",text1:""},{row:687, start:0.0446296296296296, stop:0.0446643518518519, text0:"- Why not?",text1:"- Because I am not a journalist."},{row:688, start:0.0446643518518519, stop:0.0446875, text0:"- Of course you're not.",text1:"- Well, then how can I say I am?"},{row:689, start:0.0446990740740741, stop:0.0447337962962963, text0:"Because you do. Because that's how",text1:"you get your son back, all right?"},{row:690, start:0.0447337962962963, stop:0.0447569444444444, text0:"Now get on the bus. Go.",text1:""},{row:691, start:0.0449537037037037, stop:0.0449884259259259, text0:"I am the cameraman.",text1:""},{row:692, start:0.0450578703703704, stop:0.0450810185185185, text0:"You are writing about",text1:"what is happening here?"},{row:693, start:0.0450925925925926, stop:0.0451041666666667, text0:"Yes.",text1:""},{row:694, start:0.0451041666666667, stop:0.045150462962963, text0:"So when people in your country read it,",text1:"they will come help us, yes?"},{row:695, start:0.045162037037037, stop:0.0451736111111111, text0:"Probably not.",text1:""},{row:696, start:0.0451967592592593, stop:0.0452199074074074, text0:"- You heard of a Stinger?",text1:"- What?"},{row:697, start:0.0452199074074074, stop:0.0452777777777778, text0:"Your friend, Maddy. She's like a full-on,",text1:"heat-seeking missile, bru."},{row:698, start:0.0452777777777778, stop:0.0453125, text0:"I mean, I saw her go into Afghanistan...",text1:""},{row:699, start:0.0453125, stop:0.0453472222222222, text0:"...like about 200 clicks",text1:"into Taliban land."},{row:700, start:0.0453472222222222, stop:0.0453819444444444, text0:"When the military said no,",text1:"she just took a bus, bru."},{row:701, start:0.0453819444444444, stop:0.0454166666666667, text0:"Came back with 3000 words,",text1:"and she still looked so good."},{row:702, start:0.0454282407407407, stop:0.0454398148148148, text0:"- Like hot, you know?",text1:"- All right."},{row:703, start:0.0454513888888889, stop:0.045462962962963, text0:"Calm down to the frantic now, huh?",text1:""},{row:704, start:0.0454861111111111, stop:0.0455092592592593, text0:"- What's that?",text1:"- What's going on?"},{row:705, start:0.0455208333333333, stop:0.0455324074074074, text0:"Looks like it was an RPG.",text1:""},{row:706, start:0.0455324074074074, stop:0.0455555555555556, text0:"Open the door. Let us out.",text1:""},{row:707, start:0.0455555555555556, stop:0.0455787037037037, text0:"- Let's go, let's go.",text1:"- Come on. Go, go, go."},{row:708, start:0.0456018518518519, stop:0.045625, text0:"Go, go! Move, move, move!",text1:""},{row:709, start:0.0456365740740741, stop:0.0456597222222222, text0:"We've just happened upon",text1:"the sight of an ambush..."},{row:710, start:0.0456828703703704, stop:0.0457060185185185, text0:"...fifteen kilometers",text1:"from the Liberian border..."},{row:711, start:0.0457060185185185, stop:0.0457407407407407, text0:"- ...near the Moa River...",text1:"- As you can see behind me..."},{row:712, start:0.0457407407407407, stop:0.0457523148148148, text0:"...just a few minutes ago...",text1:""},{row:713, start:0.0457638888888889, stop:0.045787037037037, text0:"- ...an ambulance has...",text1:"- Got movement in the bush."},{row:714, start:0.045787037037037, stop:0.0458101851851852, text0:"- Check out the head of the road.",text1:"- Spread out."},{row:715, start:0.0458101851851852, stop:0.0458333333333333, text0:"--at this site.",text1:"Dozens of--"},{row:716, start:0.0458449074074074, stop:0.0458680555555556, text0:"Many are injured.",text1:""},{row:717, start:0.0459837962962963, stop:0.0460185185185185, text0:"Get back on the bus, huh?",text1:"Get back on! Come on!"},{row:718, start:0.0460300925925926, stop:0.0460416666666667, text0:"- Maddy!",text1:"- Move it! Move it!"},{row:719, start:0.0460416666666667, stop:0.0460532407407407, text0:"Get back on the bus!",text1:""},{row:720, start:0.0460532407407407, stop:0.0460763888888889, text0:"Solomon! Solomon!",text1:""},{row:721, start:0.0460763888888889, stop:0.046099537037037, text0:"- Let's go.",text1:"- Solomon, come here!"},{row:722, start:0.046099537037037, stop:0.0461111111111111, text0:"- Go, go, go.",text1:"- Come on, come on."},{row:723, start:0.0461226851851852, stop:0.0461458333333333, text0:"- Get in the bus!",text1:"- Move, move, move! Come on, come on."},{row:724, start:0.0461574074074074, stop:0.0461689814814815, text0:"- Move!",text1:"- Get in the bus. Let's go."},{row:725, start:0.0461805555555556, stop:0.0462037037037037, text0:"Get in, get in.",text1:"Goddamn it, get in! Get in!"},{row:726, start:0.0462037037037037, stop:0.0462268518518519, text0:"- Let me inside!",text1:"- There's no fucking space!"},{row:727, start:0.0462384259259259, stop:0.0462615740740741, text0:"- Let them on the bus.",text1:"- You give him your seat."},{row:728, start:0.0462615740740741, stop:0.0462962962962963, text0:"What if we just take your place, huh?",text1:"What if we take your place?"},{row:729, start:0.0462962962962963, stop:0.0463310185185185, text0:"That's enough. He can ride with me.",text1:"Oi, he can ride with me."},{row:730, start:0.0463310185185185, stop:0.0463657407407407, text0:"Maddy! Let's go!",text1:"Maddy, let's go. Come on!"},{row:731, start:0.0465162037037037, stop:0.0465509259259259, text0:"Nothing like a little bang-bang",text1:"to get the heart started, Maddy."},{row:732, start:0.0465625, stop:0.0465972222222222, text0:"- Whatever you say, Corbauld.",text1:"- Come on, love. Don't be like that."},{row:733, start:0.0465972222222222, stop:0.0466319444444444, text0:"Story's played out, hasn't it?",text1:"Government bad, rebels worse."},{row:734, start:0.0466435185185185, stop:0.0466666666666667, text0:"No one gives a toss anymore.",text1:"Know what I mean?"},{row:735, start:0.0470833333333333, stop:0.0471064814814815, text0:"This medicine will make you strong...",text1:""},{row:736, start:0.0471180555555556, stop:0.0471412037037037, text0:"...make you invisible to your enemies.",text1:""},{row:737, start:0.0471412037037037, stop:0.0471759259259259, text0:"Bullets will bounce off you.",text1:""},{row:738, start:0.0472916666666667, stop:0.047337962962963, text0:"Superboy Killer, yes, huh?",text1:"Commando."},{row:739, start:0.047349537037037, stop:0.0473726851851852, text0:"Foreigners are the cause of the problems.",text1:""},{row:740, start:0.0474074074074074, stop:0.0474537037037037, text0:"Poppy Sankoh say,",text1:"'Overthrow those who would exploit you.'"},{row:741, start:0.0474768518518519, stop:0.0474884259259259, text0:"I am Baby Killer.",text1:""},{row:742, start:0.0475347222222222, stop:0.0475694444444444, text0:"- I am the Master of Disaster.",text1:"- You will be strong."},{row:743, start:0.0476157407407407, stop:0.0476273148148148, text0:"I am Born Trouble.",text1:""},{row:744, start:0.0477083333333333, stop:0.0477314814814815, text0:"I am See Me No More.",text1:""},{row:745, start:0.0478356481481481, stop:0.0478703703703704, text0:"You'd think they'd want",text1:"at least one decent road in this place."},{row:746, start:0.0478935185185185, stop:0.0479398148148148, text0:"I'm gonna have to start charging",text1:"an entrance fee. Oh, God!"},{row:747, start:0.048287037037037, stop:0.0482986111111111, text0:"Get down!",text1:""},{row:748, start:0.0483912037037037, stop:0.0484143518518519, text0:"- Look out!",text1:"- Shit!"},{row:749, start:0.0485648148148148, stop:0.0485763888888889, text0:"- You all right?",text1:"- Fine."},{row:750, start:0.0486458333333333, stop:0.0486574074074074, text0:"Shit!",text1:""},{row:751, start:0.0486921296296296, stop:0.0487152777777778, text0:"Hold on!",text1:""},{row:752, start:0.0488310185185185, stop:0.0488425925925926, text0:"Hold on!",text1:""},{row:753, start:0.0489699074074074, stop:0.0489930555555556, text0:"Come on.",text1:""},{row:754, start:0.0490740740740741, stop:0.0490972222222222, text0:"- Everyone all right, huh?",text1:"- Yeah, I'm fine."},{row:755, start:0.0491087962962963, stop:0.0491203703703704, text0:"Let me see that. You okay?",text1:""},{row:756, start:0.0491203703703704, stop:0.0491319444444444, text0:"- Yeah.",text1:"- All right."},{row:757, start:0.0491666666666667, stop:0.0492013888888889, text0:"Quietly, now. Quietly.",text1:""},{row:758, start:0.0494444444444444, stop:0.0494560185185185, text0:"Come on.",text1:""},{row:759, start:0.0497106481481481, stop:0.0497569444444444, text0:"Kamajors. Local militia",text1:"protecting their homes, huh?"},{row:760, start:0.0497569444444444, stop:0.0497800925925926, text0:"- Do not look them in the eye.",text1:"- No."},{row:761, start:0.0499421296296296, stop:0.0499652777777778, text0:"Excuse me. Excuse me?",text1:""},{row:762, start:0.0499768518518519, stop:0.0500115740740741, text0:"Hi, I'm Maddy Bowen",text1:"with Vital Affairs magazine."},{row:763, start:0.0500115740740741, stop:0.0500462962962963, text0:"Wonder if I could take your picture.",text1:"I'm just gonna get my camera."},{row:764, start:0.0500578703703704, stop:0.0500925925925926, text0:"I've heard about your struggle.",text1:"I'd love to know more about it."},{row:765, start:0.0500925925925926, stop:0.0501157407407407, text0:"Would you mind if I took your picture?",text1:"Everyone together?"},{row:766, start:0.0501273148148148, stop:0.0501388888888889, text0:"Everyone could all come in together.",text1:""},{row:767, start:0.0501851851851852, stop:0.0502083333333333, text0:"Is that okay?",text1:"If everyone could come closer."},{row:768, start:0.0502199074074074, stop:0.0502546296296296, text0:"If you come over here and come in",text1:"nice and close. That's good."},{row:769, start:0.0502546296296296, stop:0.0503009259259259, text0:"Yeah. That's great.",text1:"And we'll get one with all of us."},{row:770, start:0.0503009259259259, stop:0.0503240740740741, text0:"There you go. That's perfect.",text1:""},{row:771, start:0.0503472222222222, stop:0.0503703703703704, text0:"How about one together?",text1:"Us together, what do you think?"},{row:772, start:0.0503703703703704, stop:0.0503935185185185, text0:"I'll come in like this, yeah?",text1:""},{row:773, start:0.0504050925925926, stop:0.0504166666666667, text0:"Cheese.",text1:""},{row:774, start:0.0504398148148148, stop:0.050474537037037, text0:"So do you think",text1:"they've read your work?"},{row:775, start:0.050474537037037, stop:0.0504976851851852, text0:"Shut up.",text1:""},{row:776, start:0.0506481481481481, stop:0.0506712962962963, text0:"Hey, get them, get them, get them.",text1:""},{row:777, start:0.0506944444444445, stop:0.0507291666666667, text0:"- I got a score. I got a score.",text1:"- You missed."},{row:778, start:0.0507407407407407, stop:0.0507638888888889, text0:"Where have you come from?",text1:""},{row:779, start:0.050775462962963, stop:0.0508101851851852, text0:"- Where did you come from?",text1:"- Where have you come from?"},{row:780, start:0.0508217592592593, stop:0.0508564814814815, text0:"That's right. Yeah.",text1:"From New York. From America."},{row:781, start:0.0508564814814815, stop:0.0508796296296296, text0:"I don't have anything for you.",text1:""},{row:782, start:0.0511111111111111, stop:0.0511342592592593, text0:"My name is Benjamin Margai.",text1:""},{row:783, start:0.0511342592592593, stop:0.0511689814814815, text0:"Danny Archer. This is Maddy Bowen.",text1:""},{row:784, start:0.0511689814814815, stop:0.0512037037037037, text0:"- Hello. How are you?",text1:"- Very well, thank you."},{row:785, start:0.0513773148148148, stop:0.051400462962963, text0:"I'm the cameraman?",text1:""},{row:786, start:0.0514351851851852, stop:0.0514583333333333, text0:"The Belgians were the first",text1:"to chop off limbs in Africa."},{row:787, start:0.0514699074074074, stop:0.0515162037037037, text0:"King Leopold took one hand",text1:"for every hundredth slave in the Congo..."},{row:788, start:0.0515162037037037, stop:0.0515393518518519, text0:"...to keep them in line.",text1:""},{row:789, start:0.0515509259259259, stop:0.0515856481481481, text0:"Many of these children",text1:"were taken by the RUF."},{row:790, start:0.0515856481481481, stop:0.0516087962962963, text0:"We have taken them back.",text1:""},{row:791, start:0.0516087962962963, stop:0.0516435185185185, text0:"Some of them have been",text1:"made to do horrible things."},{row:792, start:0.0516435185185185, stop:0.0516782407407407, text0:"We are trying",text1:"to bring them back to life."},{row:793, start:0.0516898148148148, stop:0.0517013888888889, text0:"Let me show you.",text1:""},{row:794, start:0.051724537037037, stop:0.0517592592592593, text0:"Imagine doing all this, huh?",text1:""},{row:795, start:0.0517708333333333, stop:0.0517939814814815, text0:"What's your excuse?",text1:""},{row:796, start:0.0518055555555556, stop:0.0518634259259259, text0:"Well, it's gotten hard to go back",text1:"to sipping lattes and talking interest rates."},{row:797, start:0.0518634259259259, stop:0.0518981481481481, text0:"So you're an action junkie, huh?",text1:""},{row:798, start:0.0519097222222222, stop:0.0519328703703704, text0:"Three out of five ex-boyfriends",text1:"recently polled..."},{row:799, start:0.0519328703703704, stop:0.0519907407407407, text0:"...say that I prefer to be",text1:"in a constant state of crisis."},{row:800, start:0.0519907407407407, stop:0.0520138888888889, text0:"Maybe I just give a shit.",text1:""},{row:801, start:0.0520601851851852, stop:0.0520717592592593, text0:"You were good today.",text1:""},{row:802, start:0.0521064814814815, stop:0.0521180555555556, text0:"You know that?",text1:""},{row:803, start:0.0521296296296296, stop:0.0521527777777778, text0:"You too.",text1:""},{row:804, start:0.0521759259259259, stop:0.0522106481481482, text0:"'--that sometimes they forgot",text1:"to close the door."},{row:805, start:0.0522222222222222, stop:0.0522569444444444, text0:"She also wrote",text1:"about her sister Josie..."},{row:806, start:0.0522569444444444, stop:0.0523148148148148, text0:"...who was very ill",text1:"and who had to be taken to hospital.'"},{row:807, start:0.052337962962963, stop:0.052349537037037, text0:"Cheers.",text1:""},{row:808, start:0.0523842592592593, stop:0.0524189814814815, text0:"So how many attacks",text1:"have there been in the area?"},{row:809, start:0.0524189814814815, stop:0.0524537037037037, text0:"I have known most of the rebels",text1:"since they were children."},{row:810, start:0.0524537037037037, stop:0.0525115740740741, text0:"The local commander is still afraid",text1:"I will come after him with my ruler."},{row:811, start:0.0525231481481481, stop:0.0525694444444444, text0:"So you think because your intentions",text1:"are good, they'll spare you, huh?"},{row:812, start:0.0525694444444444, stop:0.0526041666666667, text0:"My heart always told me",text1:"that people are inherently good."},{row:813, start:0.0526041666666667, stop:0.0526273148148148, text0:"My experience suggests otherwise.",text1:""},{row:814, start:0.0526273148148148, stop:0.052650462962963, text0:"But what about you, Mr. Archer?",text1:""},{row:815, start:0.052650462962963, stop:0.0526851851851852, text0:"In your long career as a journalist...",text1:""},{row:816, start:0.0526851851851852, stop:0.0527199074074074, text0:"...would you say that people",text1:"are mostly good?"},{row:817, start:0.0527430555555556, stop:0.0527546296296296, text0:"No.",text1:""},{row:818, start:0.0527662037037037, stop:0.0527777777777778, text0:"I'd say they're just people.",text1:""},{row:819, start:0.0527777777777778, stop:0.0528240740740741, text0:"Exactly. It is what they do",text1:"that makes them good or bad."},{row:820, start:0.0528240740740741, stop:0.0528703703703704, text0:"A moment of love,",text1:"even in a bad man..."},{row:821, start:0.0528703703703704, stop:0.0528935185185185, text0:"...can give meaning to a life.",text1:""},{row:822, start:0.0529050925925926, stop:0.0529398148148148, text0:"None of us knows which path",text1:"will lead us to God."},{row:823, start:0.052974537037037, stop:0.0529861111111111, text0:"Hey!",text1:""},{row:824, start:0.0535416666666667, stop:0.0535648148148148, text0:"It's palm wine.",text1:""},{row:825, start:0.0536805555555556, stop:0.0537037037037037, text0:"- Jesus Christ.",text1:"- Yeah, yeah. It takes getting used to, huh?"},{row:826, start:0.0537152777777778, stop:0.0537268518518519, text0:"God.",text1:""},{row:827, start:0.0537268518518519, stop:0.0537615740740741, text0:"- Drank that as a boy.",text1:"- In Rhodesia?"},{row:828, start:0.0537847222222222, stop:0.0537962962962963, text0:"I take notes.",text1:""},{row:829, start:0.0538310185185185, stop:0.0538541666666667, text0:"The Shona...",text1:""},{row:830, start:0.0538657407407407, stop:0.0538888888888889, text0:"The Shona have a word for me.",text1:""},{row:831, start:0.053912037037037, stop:0.0539583333333333, text0:"- It means 'white boy in Africa.'",text1:"- When did you leave?"},{row:832, start:0.0539699074074074, stop:0.0539814814814815, text0:"I didn't leave.",text1:""},{row:833, start:0.0539814814814815, stop:0.0540393518518519, text0:"I got sent away to South Africa",text1:"in 1978 when the munts overran us."},{row:834, start:0.0540393518518519, stop:0.0540625, text0:"Then you joined the army?",text1:""},{row:835, start:0.0540856481481481, stop:0.0541203703703704, text0:"Yeah. Yeah, I did my time in Angola.",text1:""},{row:836, start:0.0541203703703704, stop:0.0541435185185185, text0:"- The 32 Battalion.",text1:"- That's right."},{row:837, start:0.0541782407407407, stop:0.054212962962963, text0:"You know, contrary to what",text1:"you might think about us..."},{row:838, start:0.054212962962963, stop:0.0542592592592593, text0:"...we fought with the blacks, huh?",text1:"Yeah. Side by side."},{row:839, start:0.0542592592592593, stop:0.0543055555555556, text0:"There was no apartheid in a foxhole.",text1:"The colonel always used to tell us that."},{row:840, start:0.0543055555555556, stop:0.0543402777777778, text0:"- This is Colonel Coetzee?",text1:"- Yeah."},{row:841, start:0.054375, stop:0.0544097222222222, text0:"Then, of course, it's 1994.",text1:"No more army."},{row:842, start:0.0544097222222222, stop:0.0544328703703704, text0:"No more apartheid.",text1:""},{row:843, start:0.0544328703703704, stop:0.0544675925925926, text0:"Truth and reconciliation",text1:"and all of that rubbish, you know."},{row:844, start:0.0544791666666667, stop:0.0544907407407407, text0:"Kumbaya.",text1:""},{row:845, start:0.0545023148148148, stop:0.054537037037037, text0:"We fought and died together,",text1:"you know?"},{row:846, start:0.054537037037037, stop:0.0545717592592593, text0:"Black and white. Most people back home",text1:"didn't even know we were at war."},{row:847, start:0.0545717592592593, stop:0.0545949074074074, text0:"We thought we were",text1:"fighting Communism..."},{row:848, start:0.0545949074074074, stop:0.0546412037037037, text0:"...but in the end, it was all about",text1:"who gets what, you know?"},{row:849, start:0.0546412037037037, stop:0.0546759259259259, text0:"Ivory, oil, gold:",text1:""},{row:850, start:0.0546875, stop:0.0547106481481482, text0:"Diamonds.",text1:""},{row:851, start:0.0547453703703704, stop:0.0547800925925926, text0:"So one day I decided,",text1:"'Fuck it,' you know?"},{row:852, start:0.0547916666666667, stop:0.0548032407407407, text0:"'I'm gonna get mine.'",text1:""},{row:853, start:0.0548148148148148, stop:0.0548263888888889, text0:"You gonna steal his diamond?",text1:""},{row:854, start:0.0549189814814815, stop:0.0549537037037037, text0:"That diamond is my ticket",text1:"out of this godforsaken continent."},{row:855, start:0.0549652777777778, stop:0.0549884259259259, text0:"That doesn't answer the question.",text1:""},{row:856, start:0.0551041666666667, stop:0.055150462962963, text0:"My dad came home",text1:"from the war in '69."},{row:857, start:0.0551851851851852, stop:0.0551967592592593, text0:"And it took him...",text1:""},{row:858, start:0.0552199074074074, stop:0.0552430555555556, text0:"...about 20 years to get right.",text1:""},{row:859, start:0.0552662037037037, stop:0.0552777777777778, text0:"What?",text1:""},{row:860, start:0.0553125, stop:0.0553587962962963, text0:"You Americans love to talk",text1:"about your feelings, huh?"},{row:861, start:0.0553587962962963, stop:0.0553819444444444, text0:"So, what does that mean?",text1:"What does that mean?"},{row:862, start:0.0553935185185185, stop:0.0554166666666667, text0:"- Got a thing for messed-up vets now?",text1:"- Shut up."},{row:863, start:0.0554398148148148, stop:0.0554513888888889, text0:"You lost both your parents.",text1:""},{row:864, start:0.055462962962963, stop:0.0554976851851852, text0:"That's a polite way",text1:"of putting it, yeah."},{row:865, start:0.0555324074074074, stop:0.0555555555555556, text0:"Mom was raped and shot...",text1:""},{row:866, start:0.0555902777777778, stop:0.055625, text0:"...and Dad was decapitated",text1:"and hung from a hook in the barn."},{row:867, start:0.0556365740740741, stop:0.0556597222222222, text0:"I was 9.",text1:""},{row:868, start:0.0557060185185185, stop:0.0557175925925926, text0:"Boohoo, right?",text1:""},{row:869, start:0.0557638888888889, stop:0.055787037037037, text0:"Sometimes I wonder...",text1:""},{row:870, start:0.0558217592592593, stop:0.0558564814814815, text0:"...will God ever forgive us",text1:"for what we've done to each other?"},{row:871, start:0.0559027777777778, stop:0.0559375, text0:"Then I look around and I realize...",text1:""},{row:872, start:0.0559490740740741, stop:0.0559837962962963, text0:"...God left this place a long time ago.",text1:""},{row:873, start:0.0560648148148148, stop:0.056087962962963, text0:"It's...",text1:""},{row:874, start:0.0561111111111111, stop:0.0561342592592593, text0:"What's the point, huh?",text1:""},{row:875, start:0.0566435185185185, stop:0.0566666666666667, text0:"Oh, shit. Keep moving, huh?",text1:""},{row:876, start:0.0566666666666667, stop:0.0566898148148148, text0:"- What?",text1:"- Drive at them. They'll panic."},{row:877, start:0.0566898148148148, stop:0.056712962962963, text0:"Do you know where the word",text1:"'infantry' comes from?"},{row:878, start:0.056712962962963, stop:0.0567592592592593, text0:"It means 'child soldier.'",text1:"They are just children."},{row:879, start:0.0567592592592593, stop:0.0567939814814815, text0:"- Stop! Stop! RUF on the road!",text1:"- Hi. Hello. Hello."},{row:880, start:0.0568171296296296, stop:0.0568287037037037, text0:"Hello, little brothers.",text1:""},{row:881, start:0.0568287037037037, stop:0.0568518518518519, text0:"- RUF on the road!",text1:"- Yeah, yeah. What is your names?"},{row:882, start:0.0568634259259259, stop:0.0568865740740741, text0:"I'm Dead Body.",text1:"Who is this motherfucker?"},{row:883, start:0.0568865740740741, stop:0.0569097222222222, text0:"We are teachers on our way to Kono.",text1:""},{row:884, start:0.0569097222222222, stop:0.0569328703703704, text0:"Only RUF on the road.",text1:""},{row:885, start:0.0569444444444444, stop:0.0569560185185185, text0:"- RUF only!",text1:"- Yes, yes, I understand that."},{row:886, start:0.0569675925925926, stop:0.0569791666666667, text0:"- Only RUF!",text1:"- But son, this is--"},{row:887, start:0.0570023148148148, stop:0.057025462962963, text0:"Don't move! Don't move! Fuck.",text1:""},{row:888, start:0.0570486111111111, stop:0.0570833333333333, text0:"Jesus Christ! Maddy, press your hand",text1:"against the exit wound there, huh?"},{row:889, start:0.0570949074074074, stop:0.0571180555555556, text0:"- Got it.",text1:"- Keep it there, huh?"},{row:890, start:0.0571180555555556, stop:0.0571412037037037, text0:"Whatever you do,",text1:"don't talk, all right?"},{row:891, start:0.0571412037037037, stop:0.0571643518518518, text0:"Is there any wine left in that jug?",text1:""},{row:892, start:0.0571643518518518, stop:0.0571875, text0:"- Here.",text1:"- Pour it all over everything, huh?"},{row:893, start:0.0571990740740741, stop:0.0572337962962963, text0:"The colonel's gonna have a medic",text1:"at the airstrip in Kono."},{row:894, start:0.0572453703703704, stop:0.0572800925925926, text0:"- How far is it?",text1:"- You said not to talk."},{row:895, start:0.0572800925925926, stop:0.0572916666666667, text0:"A comedian, huh?",text1:""},{row:896, start:0.0574537037037037, stop:0.0574884259259259, text0:"- How's his pulse?",text1:"- I can feel it sometimes."},{row:897, start:0.0576041666666667, stop:0.0576388888888889, text0:"- Hold it right there.",text1:"- Stop."},{row:898, start:0.0576388888888889, stop:0.057650462962963, text0:"Papers, please.",text1:""},{row:899, start:0.0577083333333333, stop:0.0577199074074074, text0:"Excuse me.",text1:""},{row:900, start:0.0577199074074074, stop:0.0577546296296296, text0:"I was just trying",text1:"to keep pressure on the wound."},{row:901, start:0.0577546296296296, stop:0.0577893518518519, text0:"- He's lost a lot of blood.",text1:"- You'll be all right, huh?"},{row:902, start:0.0577893518518519, stop:0.0578125, text0:"Will you?",text1:""},{row:903, start:0.0578125, stop:0.0578472222222222, text0:"I'll check on him in the tent",text1:"in a little bit. Thank you."},{row:904, start:0.0578587962962963, stop:0.0578703703703704, text0:"Hey.",text1:""},{row:905, start:0.0578703703703704, stop:0.0578935185185185, text0:"Look who's alive, eh?",text1:""},{row:906, start:0.0579050925925926, stop:0.0579166666666667, text0:"I told the H you'd be back.",text1:""},{row:907, start:0.0579398148148148, stop:0.057974537037037, text0:"I'm afraid we're evacuating",text1:"all non-military personnel, miss."},{row:908, start:0.057974537037037, stop:0.0579976851851852, text0:"Transport touches down at 1600.",text1:""},{row:909, start:0.0579976851851852, stop:0.0580208333333333, text0:"She's with me, all right?",text1:""},{row:910, start:0.0580208333333333, stop:0.0580671296296296, text0:"No exceptions, pal. This is gonna be ugly.",text1:"Colonel doesn't want an audience."},{row:911, start:0.0580671296296296, stop:0.0581018518518518, text0:"Is I.R.'s presence here",text1:"known to other African nations?"},{row:912, start:0.0581134259259259, stop:0.0581365740740741, text0:"- She's with you?",text1:"- Yeah."},{row:913, start:0.0581481481481482, stop:0.0581712962962963, text0:"Come on. The colonel's this way.",text1:""},{row:914, start:0.0581712962962963, stop:0.0581944444444444, text0:"Give me a minute, huh?",text1:""},{row:915, start:0.0582291666666667, stop:0.0582638888888889, text0:"We block their escape",text1:"by blowing the bridge."},{row:916, start:0.058275462962963, stop:0.0582986111111111, text0:"That's correct.",text1:""},{row:917, start:0.0583101851851852, stop:0.0583333333333333, text0:"You made it out. Well done.",text1:""},{row:918, start:0.0583333333333333, stop:0.0583564814814815, text0:"Negative, Kilo Foxtrot, stand by.",text1:""},{row:919, start:0.0583680555555556, stop:0.0584143518518518, text0:"- Colonel, sir.",text1:"- So must be real, then."},{row:920, start:0.0584259259259259, stop:0.0584375, text0:"The pink.",text1:""},{row:921, start:0.0584490740740741, stop:0.0584722222222222, text0:"Well, you're part",text1:"of this operation now."},{row:922, start:0.0584722222222222, stop:0.0585185185185185, text0:"First we snuff everything that moves",text1:"out there, then we find your diamond."},{row:923, start:0.0585185185185185, stop:0.0585416666666667, text0:"Colonel?",text1:""},{row:924, start:0.0585416666666667, stop:0.0585763888888889, text0:"Go get kitted. Yes?",text1:""},{row:925, start:0.0586458333333333, stop:0.0586689814814815, text0:"Move, move, move!",text1:""},{row:926, start:0.0586689814814815, stop:0.0587037037037037, text0:"- Go get your kit.",text1:"- Will do, chum. Will do."},{row:927, start:0.05875, stop:0.0587847222222222, text0:"It's go time, all right?",text1:"Meet me behind the tents."},{row:928, start:0.0588194444444444, stop:0.0588425925925926, text0:"How's Benjamin?",text1:""},{row:929, start:0.0588425925925926, stop:0.0588657407407407, text0:"They say he'll live.",text1:""},{row:930, start:0.0588773148148148, stop:0.0588888888888889, text0:"Good. Good.",text1:""},{row:931, start:0.0589814814814815, stop:0.0589930555555556, text0:"Listen, Maddy...",text1:""},{row:932, start:0.0590162037037037, stop:0.0590277777777778, text0:"...I need you to do me one last favor.",text1:""},{row:933, start:0.0590509259259259, stop:0.0590625, text0:"Yeah?",text1:""},{row:934, start:0.0590856481481482, stop:0.0591087962962963, text0:"Tell that guard",text1:"you wanna put him..."},{row:935, start:0.0591087962962963, stop:0.0591319444444444, text0:"...on the cover of Soldier of Fortune",text1:"magazine for me."},{row:936, start:0.0592013888888889, stop:0.059224537037037, text0:"You're going in.",text1:""},{row:937, start:0.0592708333333333, stop:0.0592939814814815, text0:"You and Solomon.",text1:""},{row:938, start:0.0593171296296296, stop:0.0593287037037037, text0:"You're going in now.",text1:""},{row:939, start:0.0593402777777778, stop:0.059375, text0:"Your plane leaves",text1:"in a few minutes, huh?"},{row:940, start:0.0593981481481481, stop:0.0594212962962963, text0:"You're an idiot.",text1:""},{row:941, start:0.0594675925925926, stop:0.0595023148148148, text0:"- What if I wanted to go in with you?",text1:"- I'd say no."},{row:942, start:0.0595023148148148, stop:0.059525462962963, text0:"- What if I insist?",text1:"- I'd insist back."},{row:943, start:0.0596064814814815, stop:0.0596180555555556, text0:"It's my story.",text1:""},{row:944, start:0.0596180555555556, stop:0.0596527777777778, text0:"Your story is Van De Kaap, all right?",text1:"That stone is mine."},{row:945, start:0.0596527777777778, stop:0.0596990740740741, text0:"I'm not leaving here without it,",text1:"you understand?"},{row:946, start:0.0597106481481481, stop:0.0597453703703704, text0:"Please, just give me two minutes.",text1:""},{row:947, start:0.0599421296296296, stop:0.0599537037037037, text0:"Yeah.",text1:""},{row:948, start:0.0600925925925926, stop:0.0601041666666667, text0:"- Hello.",text1:"- Hi."},{row:949, start:0.0601157407407407, stop:0.060150462962963, text0:"My name is Maddy Bowen.",text1:"I'm with Vital Affairs magazine."},{row:950, start:0.060150462962963, stop:0.0601736111111111, text0:"I was wondering",text1:"if I could ask you a few questions."},{row:951, start:0.0601851851851852, stop:0.0601967592592593, text0:"Well, that depends what they are.",text1:""},{row:952, start:0.0601967592592593, stop:0.0602430555555556, text0:"Oh, I see you're familiar",text1:"with our devious ways, aren't you?"},{row:953, start:0.0602546296296296, stop:0.0602777777777778, text0:"Okay, so I'll start",text1:"with something simple."},{row:954, start:0.0604282407407407, stop:0.0604513888888889, text0:"And before that you were with",text1:"the South African Defense--?"},{row:955, start:0.0604513888888889, stop:0.060474537037037, text0:"- Defense Force, correct.",text1:"- Of course."},{row:956, start:0.060474537037037, stop:0.0605208333333333, text0:"And was that with Colonel Coetzee?",text1:"Were you in Angola?"},{row:957, start:0.0605208333333333, stop:0.0605324074074074, text0:"Angola, yeah.",text1:""},{row:958, start:0.0605555555555556, stop:0.0605902777777778, text0:"That is all I really need to know.",text1:"Thank you so much."},{row:959, start:0.0605902777777778, stop:0.0606134259259259, text0:"- Thank you.",text1:"- Okay."},{row:960, start:0.0607291666666667, stop:0.0607407407407407, text0:"Thank you, huh?",text1:""},{row:961, start:0.0607407407407407, stop:0.060775462962963, text0:"Just keeping my end of the deal.",text1:""},{row:962, start:0.060775462962963, stop:0.0607986111111111, text0:"Good luck to you, Solomon.",text1:""},{row:963, start:0.0607986111111111, stop:0.0608101851851852, text0:"Okay.",text1:""},{row:964, start:0.0608217592592593, stop:0.0608680555555556, text0:"- I hope you find what you're looking for.",text1:"- Thank you for all you have done."},{row:965, start:0.0609606481481481, stop:0.0609837962962963, text0:"- Listen, I--",text1:"- No, you listen."},{row:966, start:0.0610416666666667, stop:0.0610648148148148, text0:"In another life maybe, all right?",text1:""},{row:967, start:0.0610763888888889, stop:0.061099537037037, text0:"Yeah. Sure.",text1:""},{row:968, start:0.061099537037037, stop:0.0611226851851852, text0:"Yeah.",text1:""},{row:969, start:0.0611689814814815, stop:0.0611921296296296, text0:"You find yourself a good man,",text1:"all right, Maddy?"},{row:970, start:0.0612615740740741, stop:0.0612962962962963, text0:"You know, I've got three sisters.",text1:""},{row:971, start:0.0612962962962963, stop:0.0613310185185185, text0:"They're all married to good men.",text1:""},{row:972, start:0.0613541666666667, stop:0.0613657407407407, text0:"I prefer my life.",text1:""},{row:973, start:0.061400462962963, stop:0.0614467592592593, text0:"Here is my office phone...",text1:""},{row:974, start:0.0614467592592593, stop:0.0614814814814815, text0:"...home phone, cellphone.",text1:""},{row:975, start:0.0615046296296296, stop:0.0615393518518519, text0:"I'm used to being pursued,",text1:"but what the heck."},{row:976, start:0.0616666666666667, stop:0.0616898148148148, text0:"You better get on that plane, huh?",text1:""},{row:977, start:0.061712962962963, stop:0.0617361111111111, text0:"You should get on that plane.",text1:""},{row:978, start:0.0618287037037037, stop:0.0618518518518519, text0:"So should you.",text1:""},{row:979, start:0.0619212962962963, stop:0.0619328703703704, text0:"You gonna call me?",text1:""},{row:980, start:0.0619791666666667, stop:0.0620023148148148, text0:"Soon as I'm near a phone, yeah.",text1:""},{row:981, start:0.0620138888888889, stop:0.062037037037037, text0:"Yeah, right.",text1:""},{row:982, start:0.0622916666666667, stop:0.0623263888888889, text0:"- Where is he?",text1:"- I don't know."},{row:983, start:0.0623726851851852, stop:0.0623958333333333, text0:"Oh, shit.",text1:""},{row:984, start:0.0623958333333333, stop:0.0624305555555556, text0:"Nabil? Yeah, I'll call you",text1:"when we get closer, all right?"},{row:985, start:0.0624305555555556, stop:0.0624652777777778, text0:"You keep me posted. Right.",text1:""},{row:986, start:0.0624652777777778, stop:0.0625115740740741, text0:"All right. I set the pace, huh?",text1:"Two-hour marches, five-minute breaks."},{row:987, start:0.0625115740740741, stop:0.0625578703703704, text0:"If I hold my hand up like this, you stop.",text1:"If I point to the dirt, you lie flat."},{row:988, start:0.0625578703703704, stop:0.0626041666666667, text0:"If I say go, you move like your life",text1:"depended on it. Understood?"},{row:989, start:0.0626041666666667, stop:0.0626273148148148, text0:"- Yes, boss.",text1:"- Come on."},{row:990, start:0.0627546296296296, stop:0.0627662037037037, text0:"Tell me about the stone.",text1:""},{row:991, start:0.0627777777777778, stop:0.0628009259259259, text0:"Is it clear or is it milky?",text1:""},{row:992, start:0.0628009259259259, stop:0.0628240740740741, text0:"Clear.",text1:""},{row:993, start:0.0628240740740741, stop:0.0628472222222222, text0:"And the color, is it strong?",text1:""},{row:994, start:0.0628587962962963, stop:0.0628703703703704, text0:"Does it hold the light?",text1:""},{row:995, start:0.0628703703703704, stop:0.0628935185185185, text0:"It is as you say.",text1:""},{row:996, start:0.0629050925925926, stop:0.0629398148148148, text0:"- About the size of a bird's egg?",text1:"- Yes."},{row:997, start:0.0629398148148148, stop:0.0629861111111111, text0:"One hundred carats.",text1:"Come on, let's pick up the pace."},{row:998, start:0.0634722222222222, stop:0.0634837962962963, text0:"Dia!",text1:""},{row:999, start:0.0635069444444444, stop:0.0635300925925926, text0:"Stop the truck!",text1:""},{row:1000, start:0.0635416666666667, stop:0.0635532407407407, text0:"I thought it was Dia.",text1:""},{row:1001, start:0.0641782407407407, stop:0.0642013888888889, text0:"Wake up.",text1:""},{row:1002, start:0.064224537037037, stop:0.0642592592592593, text0:"Don't worry, they're gone, huh?",text1:""},{row:1003, start:0.0642708333333333, stop:0.0643055555555556, text0:"You know, I once had this buddy...",text1:""},{row:1004, start:0.0643055555555556, stop:0.0643287037037037, text0:"...named Mabhoko.",text1:""},{row:1005, start:0.0643402777777778, stop:0.064375, text0:"We used to hunt bush meat",text1:"together as kids, huh?"},{row:1006, start:0.0643865740740741, stop:0.0643981481481481, text0:"Baboons.",text1:""},{row:1007, start:0.0644097222222222, stop:0.0644328703703704, text0:"Baboons, they were",text1:"the hardest to catch. They're..."},{row:1008, start:0.0644560185185185, stop:0.0644675925925926, text0:"They're cunning.",text1:""},{row:1009, start:0.0644675925925926, stop:0.0644907407407407, text0:"Cunning creatures.",text1:""},{row:1010, start:0.0644907407407407, stop:0.064525462962963, text0:"Fast, strong.",text1:""},{row:1011, start:0.064525462962963, stop:0.0645486111111111, text0:"Got good eyesight.",text1:""},{row:1012, start:0.0645601851851852, stop:0.0646064814814815, text0:"We would always find them",text1:"by the smell of their shit."},{row:1013, start:0.0646064814814815, stop:0.0646643518518519, text0:"And that's how we learned to track",text1:"your black terrorists in Angola, huh?"},{row:1014, start:0.0647106481481481, stop:0.0647337962962963, text0:"By the smell of your shit.",text1:""},{row:1015, start:0.0647453703703704, stop:0.0647569444444445, text0:"It's not...",text1:""},{row:1016, start:0.0647569444444445, stop:0.0647916666666667, text0:"It's not the same as a baboon's.",text1:""},{row:1017, start:0.0648148148148148, stop:0.0648611111111111, text0:"But, you know, after you skin it...",text1:""},{row:1018, start:0.0648726851851852, stop:0.0649074074074074, text0:"...the flesh of a baboon...",text1:""},{row:1019, start:0.0649074074074074, stop:0.0649305555555556, text0:"...isn't that much different",text1:"than a man's, you know."},{row:1020, start:0.0649768518518519, stop:0.0650231481481481, text0:"I'll tell you, I can track anything.",text1:""},{row:1021, start:0.065162037037037, stop:0.0651851851851852, text0:"You risk my life like that again...",text1:""},{row:1022, start:0.0652430555555556, stop:0.0652662037037037, text0:"...and I'll peel your face",text1:"back off your head."},{row:1023, start:0.0653009259259259, stop:0.0653240740740741, text0:"Do you understand?",text1:""},{row:1024, start:0.0654166666666667, stop:0.0654513888888889, text0:"Make us a fire.",text1:""},{row:1025, start:0.0662615740740741, stop:0.0662731481481482, text0:"What'd he say?",text1:""},{row:1026, start:0.0662847222222222, stop:0.0663078703703704, text0:"We are going the right way.",text1:""},{row:1027, start:0.0663888888888889, stop:0.0664236111111111, text0:"Hold up there, my man.",text1:"It's this way, huh?"},{row:1028, start:0.0664351851851852, stop:0.0664583333333333, text0:"Where do you think you are going?",text1:""},{row:1029, start:0.0664814814814815, stop:0.0665046296296296, text0:"Stop!",text1:""},{row:1030, start:0.0665046296296296, stop:0.0665277777777778, text0:"I said, stop!",text1:""},{row:1031, start:0.0665277777777778, stop:0.0665509259259259, text0:"He is not down there, all right?",text1:""},{row:1032, start:0.0665740740740741, stop:0.0666087962962963, text0:"All right. You're gonna need",text1:"some of that old discipline, huh?"},{row:1033, start:0.0666087962962963, stop:0.0666435185185185, text0:"Now you listen here, my boy,",text1:"and you listen well."},{row:1034, start:0.0666435185185185, stop:0.0666666666666667, text0:"You are not going down there.",text1:"Are we clear?"},{row:1035, start:0.0666666666666667, stop:0.0666898148148148, text0:"You are not the master.",text1:""},{row:1036, start:0.0666898148148148, stop:0.066724537037037, text0:"Right now, that is exactly what I am,",text1:"and you'd better remember it, Kaffir!"},{row:1037, start:0.0669097222222222, stop:0.0669212962962963, text0:"Don't you fucking move, huh?",text1:""},{row:1038, start:0.0669212962962963, stop:0.0669444444444444, text0:"Don't you move. Don't you move, huh?",text1:""},{row:1039, start:0.0669444444444444, stop:0.0669791666666667, text0:"- My son is there!",text1:"- Your son is gone!"},{row:1040, start:0.0669791666666667, stop:0.0669907407407407, text0:"- He is alive!",text1:"- He's gone!"},{row:1041, start:0.0669907407407407, stop:0.067025462962963, text0:"He's alive! He's there",text1:"with those crazy people!"},{row:1042, start:0.067037037037037, stop:0.0670717592592593, text0:"I am his father! His father!",text1:"I should have protected him!"},{row:1043, start:0.0670833333333333, stop:0.0671296296296296, text0:"I must go find him.",text1:"I cannot live without knowing!"},{row:1044, start:0.0671296296296296, stop:0.0671527777777778, text0:"Shoot me if you want, but I'm going.",text1:""},{row:1045, start:0.0671759259259259, stop:0.0672222222222222, text0:"Why should it matter,",text1:"I'm dead already."},{row:1046, start:0.0673148148148148, stop:0.067337962962963, text0:"We wait until dark, huh?",text1:""},{row:1047, start:0.0676736111111111, stop:0.0676851851851852, text0:"Are you satisfied?",text1:""},{row:1048, start:0.0677199074074074, stop:0.0677546296296296, text0:"All right, come on.",text1:""},{row:1049, start:0.0678125, stop:0.0678356481481481, text0:"--is fighting for Sierra Leone",text1:""},{row:1050, start:0.0678356481481481, stop:0.0678703703703704, text0:"RUF is fighting for Sierra Leone",text1:""},{row:1051, start:0.0678703703703704, stop:0.0679050925925926, text0:"Go tell my parents",text1:"They may see me no more"},{row:1052, start:0.0679050925925926, stop:0.0679398148148148, text0:"In the battlefield",text1:"We're fighting forever"},{row:1053, start:0.0679513888888889, stop:0.0679861111111111, text0:"Go tell my parents",text1:"They may see me no more"},{row:1054, start:0.0679861111111111, stop:0.0679976851851852, text0:"In the battlefield...",text1:""},{row:1055, start:0.0680324074074074, stop:0.0680555555555555, text0:"You are how old?",text1:""},{row:1056, start:0.0680555555555555, stop:0.0680787037037037, text0:"Me? I'm 31.",text1:""},{row:1057, start:0.0680787037037037, stop:0.0681018518518519, text0:"And you have no wife?",text1:""},{row:1058, start:0.0681018518518519, stop:0.0681365740740741, text0:"- No.",text1:"- No children?"},{row:1059, start:0.0681597222222222, stop:0.0681712962962963, text0:"No.",text1:""},{row:1060, start:0.0681712962962963, stop:0.0681944444444444, text0:"And no home?",text1:""},{row:1061, start:0.0681944444444444, stop:0.0682060185185185, text0:"No.",text1:""},{row:1062, start:0.0682175925925926, stop:0.0682523148148148, text0:"But you have money, yes?",text1:""},{row:1063, start:0.0682523148148148, stop:0.068275462962963, text0:"Yeah, some.",text1:""},{row:1064, start:0.068275462962963, stop:0.0682986111111111, text0:"But not enough?",text1:""},{row:1065, start:0.0683333333333333, stop:0.0683449074074074, text0:"No.",text1:""},{row:1066, start:0.0683564814814815, stop:0.0684027777777778, text0:"If you get this diamond,",text1:"you will have enough money, yes?"},{row:1067, start:0.0684375, stop:0.0684490740740741, text0:"Yes.",text1:""},{row:1068, start:0.0684490740740741, stop:0.0684837962962963, text0:"Then you will get",text1:"a wife and children?"},{row:1069, start:0.0684837962962963, stop:0.0684953703703704, text0:"Probably not, no.",text1:""},{row:1070, start:0.0685185185185185, stop:0.0685300925925926, text0:"What?",text1:""},{row:1071, start:0.0685532407407407, stop:0.0685763888888889, text0:"What?",text1:""},{row:1072, start:0.0685763888888889, stop:0.068599537037037, text0:"I am confused.",text1:""},{row:1073, start:0.0686111111111111, stop:0.0686342592592593, text0:"Yeah?",text1:""},{row:1074, start:0.0686342592592593, stop:0.0686689814814815, text0:"That makes two of us, my bru.",text1:""},{row:1075, start:0.0686921296296296, stop:0.0687384259259259, text0:"Oh, Christ.",text1:"I've gotta quit smoking, huh?"},{row:1076, start:0.0687384259259259, stop:0.0687615740740741, text0:"Why don't you?",text1:""},{row:1077, start:0.0687731481481482, stop:0.0687962962962963, text0:"I'll tell you what, Solomon.",text1:""},{row:1078, start:0.0687962962962963, stop:0.0688541666666667, text0:"You find me this diamond,",text1:"and I'll quit right then and there, huh?"},{row:1079, start:0.0689351851851852, stop:0.0689699074074074, text0:"- You were born in Zimbabwe?",text1:"- Oh, yeah."},{row:1080, start:0.0689699074074074, stop:0.0690162037037037, text0:"- Is it beautiful there?",text1:"- Sure you're not a reporter?"},{row:1081, start:0.0690162037037037, stop:0.0690625, text0:"Because you ask a hell of a lot",text1:"of questions, my man."},{row:1082, start:0.0691435185185185, stop:0.0691898148148148, text0:"My grandfather told",text1:"many stories of war."},{row:1083, start:0.0691898148148148, stop:0.069224537037037, text0:"How the Mende fought the Temne.",text1:""},{row:1084, start:0.069224537037037, stop:0.0692708333333333, text0:"Or how two tribes would fight",text1:"when somebody had stolen a woman."},{row:1085, start:0.0693865740740741, stop:0.0694212962962963, text0:"I understand white people",text1:"want our diamonds, yes?"},{row:1086, start:0.0694560185185185, stop:0.0695023148148148, text0:"But how can my own people",text1:"do this to each other?"},{row:1087, start:0.0695949074074074, stop:0.0696180555555556, text0:"I know good people...",text1:""},{row:1088, start:0.0696296296296296, stop:0.0696527777777778, text0:"...who say there is something",text1:"wrong with us..."},{row:1089, start:0.0696643518518519, stop:0.0696759259259259, text0:"...inside our black skin.",text1:""},{row:1090, start:0.0696759259259259, stop:0.0697106481481481, text0:"That we were better off",text1:"when the white man ruled."},{row:1091, start:0.0697337962962963, stop:0.0697685185185185, text0:"But my son is good.",text1:""},{row:1092, start:0.0697916666666667, stop:0.0698148148148148, text0:"And when he grows up...",text1:""},{row:1093, start:0.0698148148148148, stop:0.0698263888888889, text0:"...and peace comes...",text1:""},{row:1094, start:0.0698726851851852, stop:0.0699074074074074, text0:"...this place will be a paradise.",text1:""},{row:1095, start:0.07, stop:0.0700231481481482, text0:"We'll get your son back, huh?",text1:""},{row:1096, start:0.0705092592592593, stop:0.0705324074074074, text0:"Where is it exactly?",text1:""},{row:1097, start:0.0705555555555556, stop:0.0705902777777778, text0:"Through the camp, by the river.",text1:""},{row:1098, start:0.0706365740740741, stop:0.0706597222222222, text0:"Christ.",text1:""},{row:1099, start:0.0706712962962963, stop:0.0706828703703704, text0:"Okay.",text1:""},{row:1100, start:0.0706944444444444, stop:0.0707175925925926, text0:"I'm gonna make contact",text1:"with the colonel."},{row:1101, start:0.0707175925925926, stop:0.0707291666666667, text0:"He'll send in an air strike.",text1:""},{row:1102, start:0.0707407407407407, stop:0.070775462962963, text0:"- What is that?",text1:"- A chopper, huh? An attack helicopter."},{row:1103, start:0.070775462962963, stop:0.0708101851851852, text0:"- We use it as a diversion.",text1:"- What if my son is there?"},{row:1104, start:0.0708101851851852, stop:0.0708217592592593, text0:"- He isn't.",text1:"- How do you know?"},{row:1105, start:0.0708217592592593, stop:0.0708680555555555, text0:"I said we do it my way now, all right?",text1:"Don't fuck with me."},{row:1106, start:0.0709837962962963, stop:0.0710069444444444, text0:"Cordell, it's Danny.",text1:""},{row:1107, start:0.0710069444444444, stop:0.0710416666666667, text0:"Yeah, yeah, yeah.",text1:"Shut up and listen, all right?"},{row:1108, start:0.0710416666666667, stop:0.0710763888888889, text0:"You tell the colonel I've found the stone.",text1:""},{row:1109, start:0.0710763888888889, stop:0.0711111111111111, text0:"No, I haven't got it in my hands,",text1:"but I'm OP'ing its location."},{row:1110, start:0.0711111111111111, stop:0.0711342592592593, text0:"It's a major rebel camp, my man.",text1:""},{row:1111, start:0.0711342592592593, stop:0.0711574074074074, text0:"You tell him to send in the dakadak.",text1:""},{row:1112, start:0.0711689814814815, stop:0.0712037037037037, text0:"Because I stole a GPS, that's how.",text1:""},{row:1113, start:0.0712037037037037, stop:0.0712268518518519, text0:"If you shut up and listen,",text1:"I could give the coordinates."},{row:1114, start:0.0712384259259259, stop:0.0712731481481482, text0:"Zero-nine-zero-five-four-two-four, north.",text1:""},{row:1115, start:0.0712731481481482, stop:0.0713194444444444, text0:"Three-two-seven-two-nine",text1:"three-zero-five-zero, east."},{row:1116, start:0.0713310185185185, stop:0.0713541666666667, text0:"I'll call back at midnight to confirm.",text1:""},{row:1117, start:0.0713888888888889, stop:0.0714236111111111, text0:"You can look all night,",text1:"my friend, right?"},{row:1118, start:0.0714236111111111, stop:0.0714467592592593, text0:"I promise you he is not there.",text1:""},{row:1119, start:0.0715162037037037, stop:0.0715393518518519, text0:"They're coming in the morning, all right?",text1:""},{row:1120, start:0.0715393518518519, stop:0.0715740740740741, text0:"No movement, my friend. You stay still.",text1:""},{row:1121, start:0.0715856481481481, stop:0.0716203703703704, text0:"When it gets dark,",text1:"then we move in closer."},{row:1122, start:0.071875, stop:0.0719097222222222, text0:"Solomon. Shit.",text1:""},{row:1123, start:0.072037037037037, stop:0.0720486111111111, text0:"I can do it.",text1:""},{row:1124, start:0.0720486111111111, stop:0.0720601851851852, text0:"- Help me.",text1:"- Help him."},{row:1125, start:0.0720717592592593, stop:0.0720949074074074, text0:"Thank you, Papa.",text1:""},{row:1126, start:0.0725810185185185, stop:0.0726041666666667, text0:"Hey, you smell.",text1:""},{row:1127, start:0.0726157407407407, stop:0.0726388888888889, text0:"That's fine.",text1:""},{row:1128, start:0.0727777777777778, stop:0.0728009259259259, text0:"- Ready? I'm winning.",text1:"- Okay, let's go."},{row:1129, start:0.0728009259259259, stop:0.0728240740740741, text0:"Play, man.",text1:""},{row:1130, start:0.0728356481481481, stop:0.0728703703703704, text0:"- Play. Play the card.",text1:"- Play."},{row:1131, start:0.0731828703703704, stop:0.0731944444444444, text0:"Dia.",text1:""},{row:1132, start:0.0732291666666667, stop:0.0732523148148148, text0:"Leave me alone.",text1:""},{row:1133, start:0.0732638888888889, stop:0.0732986111111111, text0:"- Dia. I'm your papa. Come with me.",text1:"- Hey."},{row:1134, start:0.0732986111111111, stop:0.0733217592592593, text0:"- Dia.",text1:"- Get away from me."},{row:1135, start:0.0733217592592593, stop:0.0733449074074074, text0:"- Dia. Dia!",text1:"- I do not know you. Traitor!"},{row:1136, start:0.0733449074074074, stop:0.0733796296296296, text0:"- Enemy! Enemy!",text1:"- What the hell are you doing?"},{row:1137, start:0.0733796296296296, stop:0.0733912037037037, text0:"- What are you doing?",text1:"- I don't know you!"},{row:1138, start:0.0733912037037037, stop:0.0734143518518519, text0:"- What's wrong?",text1:"- I hate you!"},{row:1139, start:0.0734143518518519, stop:0.0734375, text0:"- Dia, come with me!",text1:"- I hate you!"},{row:1140, start:0.0734375, stop:0.0734490740740741, text0:"Get away from me!",text1:""},{row:1141, start:0.0734837962962963, stop:0.0735069444444444, text0:"- Get him!",text1:"- Don't shoot!"},{row:1142, start:0.0735069444444444, stop:0.0735300925925926, text0:"- Don't shoot! Don't--! Don't shoot!",text1:"- I hate you!"},{row:1143, start:0.0735416666666667, stop:0.0735532407407407, text0:"- Fisherman! Traitor!",text1:"- Don't shoot!"},{row:1144, start:0.0735648148148148, stop:0.073587962962963, text0:"Get away from me!",text1:""},{row:1145, start:0.0736689814814815, stop:0.0736921296296296, text0:"I knew you would come back.",text1:""},{row:1146, start:0.0737037037037037, stop:0.0737384259259259, text0:"I have dug and dug and dug,",text1:"but I've found nothing."},{row:1147, start:0.0737384259259259, stop:0.0737615740740741, text0:"Nothing.",text1:""},{row:1148, start:0.0737615740740741, stop:0.0737847222222222, text0:"Dig, dig, dig!",text1:""},{row:1149, start:0.0737847222222222, stop:0.0738078703703704, text0:"You will find it for me.",text1:""},{row:1150, start:0.0738194444444444, stop:0.0738541666666667, text0:"You will dig up",text1:"what you have come back for."},{row:1151, start:0.0738541666666667, stop:0.0738657407407407, text0:"No.",text1:""},{row:1152, start:0.0738657407407407, stop:0.0738888888888889, text0:"Why should the diamond",text1:"go to the foreign devils?"},{row:1153, start:0.073900462962963, stop:0.0739351851851852, text0:"We will use it",text1:"to make the country strong."},{row:1154, start:0.0739467592592593, stop:0.0739814814814815, text0:"RUF is fighting for the people!",text1:""},{row:1155, start:0.0740046296296296, stop:0.0740393518518519, text0:"RUF is fighting for Sierra Leone!",text1:""},{row:1156, start:0.0740740740740741, stop:0.0740972222222222, text0:"Pick it up.",text1:""},{row:1157, start:0.0741087962962963, stop:0.0741435185185185, text0:"Why? You are going to kill me anyway.",text1:""},{row:1158, start:0.0741435185185185, stop:0.0741782407407407, text0:"I do not need to kill you,",text1:"Solomon Vandy."},{row:1159, start:0.074212962962963, stop:0.0742361111111111, text0:"If you do not bring me the diamond...",text1:""},{row:1160, start:0.0742361111111111, stop:0.0742592592592593, text0:"...I will find the rest of your family.",text1:""},{row:1161, start:0.0742708333333333, stop:0.0742939814814815, text0:"Just as I have found your son.",text1:""},{row:1162, start:0.0743055555555556, stop:0.0743518518518518, text0:"I will rape your wife",text1:"in front of your eyes..."},{row:1163, start:0.0743518518518518, stop:0.0743865740740741, text0:"...slit her throat...",text1:""},{row:1164, start:0.0743865740740741, stop:0.0744212962962963, text0:"...and I will keep your daughters",text1:"for myself."},{row:1165, start:0.0744791666666667, stop:0.0745023148148148, text0:"You think I am a devil...",text1:""},{row:1166, start:0.0745138888888889, stop:0.074537037037037, text0:"...but only because I have lived in hell.",text1:""},{row:1167, start:0.0745601851851852, stop:0.0745833333333333, text0:"I want to get out.",text1:""},{row:1168, start:0.0745949074074074, stop:0.0746180555555556, text0:"You will help me.",text1:""},{row:1169, start:0.0746527777777778, stop:0.0746643518518518, text0:"I don't remember.",text1:""},{row:1170, start:0.0746643518518518, stop:0.0746990740740741, text0:"Find it or your family will die!",text1:""},{row:1171, start:0.0747106481481482, stop:0.0747337962962963, text0:"Now pick it up!",text1:""},{row:1172, start:0.0749189814814815, stop:0.0749305555555556, text0:"- Go!",text1:"- Bastards!"},{row:1173, start:0.0749305555555556, stop:0.0749537037037037, text0:"Shoot it!",text1:""},{row:1174, start:0.0750115740740741, stop:0.0750231481481481, text0:"Dia!",text1:""},{row:1175, start:0.075162037037037, stop:0.0751851851851852, text0:"Come, boy. Come. This way. This way.",text1:""},{row:1176, start:0.0751967592592593, stop:0.0752083333333333, text0:"Dia!",text1:""},{row:1177, start:0.075474537037037, stop:0.0754861111111111, text0:"Colonel, we think Archer's there.",text1:""},{row:1178, start:0.0754976851851852, stop:0.0755208333333333, text0:"I don't give a damn who's down there.",text1:"Kill them all."},{row:1179, start:0.0756481481481481, stop:0.0756597222222222, text0:"Dia!",text1:""},{row:1180, start:0.0758333333333333, stop:0.0758449074074074, text0:"I'm coming!",text1:""},{row:1181, start:0.0758912037037037, stop:0.0759027777777778, text0:"- Dia!",text1:"- Bastard!"},{row:1182, start:0.0761689814814815, stop:0.0761921296296296, text0:"No!",text1:""},{row:1183, start:0.0764236111111111, stop:0.0764351851851852, text0:"Dia!",text1:""},{row:1184, start:0.0771990740740741, stop:0.0772337962962963, text0:"- We found his digger, sir.",text1:"- Keep him close."},{row:1185, start:0.0772569444444444, stop:0.0772916666666667, text0:"- Danny boy.",text1:"- Colonel."},{row:1186, start:0.0772916666666667, stop:0.0773263888888889, text0:"- Thanks for calling it in.",text1:"- Yeah, yeah. Always happy to help, huh?"},{row:1187, start:0.0773263888888889, stop:0.0773726851851852, text0:"So where is it? The stone?",text1:""},{row:1188, start:0.0773726851851852, stop:0.0773958333333333, text0:"We split, huh? Fifty-fifty, all right?",text1:""},{row:1189, start:0.0773958333333333, stop:0.0774074074074074, text0:"Look around you, Danny boy.",text1:""},{row:1190, start:0.0774189814814815, stop:0.0774537037037037, text0:"You're not in a position",text1:"to be dictating terms."},{row:1191, start:0.0774537037037037, stop:0.0774768518518519, text0:"Then you will never find it, sir.",text1:""},{row:1192, start:0.0774768518518519, stop:0.0775, text0:"That's correct.",text1:""},{row:1193, start:0.0775115740740741, stop:0.0775462962962963, text0:"- But he will.",text1:"- Come. Up."},{row:1194, start:0.0775694444444445, stop:0.0775925925925926, text0:"Introduce me to your digger.",text1:""},{row:1195, start:0.0776041666666667, stop:0.0776273148148148, text0:"Solomon Vandy.",text1:""},{row:1196, start:0.0776273148148148, stop:0.077650462962963, text0:"This is Colonel Coetzee.",text1:"He wants the diamond."},{row:1197, start:0.077662037037037, stop:0.0776967592592593, text0:"- No more than you.",text1:"- Fuck."},{row:1198, start:0.0776967592592593, stop:0.0777199074074074, text0:"You gonna show us where it's hidden?",text1:""},{row:1199, start:0.0777430555555556, stop:0.0777662037037037, text0:"Does he speak English?",text1:""},{row:1200, start:0.0777662037037037, stop:0.0778009259259259, text0:"He will only tell me, sir.",text1:"I promise you that."},{row:1201, start:0.0778125, stop:0.0778472222222222, text0:"- Let me talk to him, huh?",text1:"- Go on."},{row:1202, start:0.0779282407407407, stop:0.0779513888888889, text0:"All right, my friend.",text1:""},{row:1203, start:0.077962962962963, stop:0.0779861111111111, text0:"End of the line here, huh?",text1:""},{row:1204, start:0.0779861111111111, stop:0.0780208333333333, text0:"- You just tell him where it is, all right?",text1:"- I do not trust him."},{row:1205, start:0.0780208333333333, stop:0.0780671296296296, text0:"I don't trust him either,",text1:"but we haven't got much of a choice here."},{row:1206, start:0.0780787037037037, stop:0.0781134259259259, text0:"I promise you",text1:"he will shoot us both in the head."},{row:1207, start:0.078125, stop:0.0781597222222222, text0:"Right, tell him, huh?",text1:"Tell him where it is, huh?"},{row:1208, start:0.0781597222222222, stop:0.0781828703703704, text0:"- Go on. Come on.",text1:"- I'm not feeling the love, Danny."},{row:1209, start:0.0782060185185185, stop:0.0782291666666667, text0:"All right.",text1:"No more messing about, all right?"},{row:1210, start:0.0782407407407407, stop:0.078275462962963, text0:"I said tell him where it is!",text1:"Tell him where it is! Now!"},{row:1211, start:0.078275462962963, stop:0.078287037037037, text0:"Tell him!",text1:""},{row:1212, start:0.0783101851851852, stop:0.0783217592592593, text0:"- No.",text1:"- Fucking--"},{row:1213, start:0.0783333333333333, stop:0.0783564814814815, text0:"Danny, there are other ways",text1:"to do this, okay?"},{row:1214, start:0.0783564814814815, stop:0.0783796296296296, text0:"Yeah, yeah.",text1:""},{row:1215, start:0.0784722222222222, stop:0.0785069444444444, text0:"- If he tells us, do we have a deal, sir?",text1:"- Seventy-thirty."},{row:1216, start:0.0785069444444444, stop:0.0785416666666667, text0:"Sixty-forty. I've already lined up a buyer.",text1:""},{row:1217, start:0.0785416666666667, stop:0.0785648148148148, text0:"Agreed.",text1:""},{row:1218, start:0.0786226851851852, stop:0.0786342592592593, text0:"Come here.",text1:""},{row:1219, start:0.0786689814814815, stop:0.0787037037037037, text0:"This is his son. Vandy will do",text1:"whatever you want for his sake, all right?"},{row:1220, start:0.0787152777777778, stop:0.0787615740740741, text0:"Danny, you're a pisser, man. Well done.",text1:"Boys, we move out."},{row:1221, start:0.0787962962962963, stop:0.0788194444444444, text0:"What do you say, Mr. Vandy?",text1:""},{row:1222, start:0.0788310185185185, stop:0.0788657407407408, text0:"- You ready to take a walk?",text1:"- Come on."},{row:1223, start:0.0789351851851852, stop:0.0789583333333333, text0:"Come on.",text1:""},{row:1224, start:0.0791087962962963, stop:0.0791435185185185, text0:"So who's the buyer?",text1:"I'm guessing London."},{row:1225, start:0.0791435185185185, stop:0.0791782407407407, text0:"Well, they no longer accept",text1:"conflict diamonds, huh?"},{row:1226, start:0.0791782407407407, stop:0.079212962962963, text0:"Wouldn't hurt to interest",text1:"some other parties."},{row:1227, start:0.079212962962963, stop:0.0792476851851852, text0:"- Start a bidding war.",text1:"- What, one war's not enough for you, huh?"},{row:1228, start:0.0792592592592593, stop:0.0792824074074074, text0:"I missed you, Danny.",text1:""},{row:1229, start:0.0792824074074074, stop:0.0793287037037037, text0:"See? It is here.",text1:"He has tried to find it."},{row:1230, start:0.0793402777777778, stop:0.0793634259259259, text0:"All right, Mr. Vandy.",text1:"Time for you to start digging."},{row:1231, start:0.079537037037037, stop:0.0795717592592593, text0:"Hey, boet, have you got a smoke?",text1:""},{row:1232, start:0.0795717592592593, stop:0.0795949074074074, text0:"Smoking will kill you.",text1:""},{row:1233, start:0.0796064814814815, stop:0.0796180555555556, text0:"Sorry.",text1:""},{row:1234, start:0.0796527777777778, stop:0.0796759259259259, text0:"It's all right.",text1:""},{row:1235, start:0.0796759259259259, stop:0.0797106481481481, text0:"Time I quit smoking anyway,",text1:"huh, Solomon?"},{row:1236, start:0.0798726851851852, stop:0.0798842592592593, text0:"- It's gone.",text1:"- What?"},{row:1237, start:0.0798958333333333, stop:0.0799189814814815, text0:"Someone has taken it.",text1:""},{row:1238, start:0.0799305555555556, stop:0.0799768518518518, text0:"This is where I buried it.",text1:"It isn't here."},{row:1239, start:0.0799768518518518, stop:0.0800115740740741, text0:"Well, then there's no reason",text1:"for any of you to stay alive. Is there?"},{row:1240, start:0.0800694444444444, stop:0.0800810185185185, text0:"Wait, wait, wait.",text1:""},{row:1241, start:0.0800925925925926, stop:0.0801157407407407, text0:"I know, I know. I know where it is.",text1:""},{row:1242, start:0.0801157407407407, stop:0.0801388888888889, text0:"- I know where it is.",text1:"- It better be there."},{row:1243, start:0.0801967592592593, stop:0.0802083333333333, text0:"Yes.",text1:""},{row:1244, start:0.0802199074074074, stop:0.0802546296296296, text0:"Oh, here, here.",text1:"It is here."},{row:1245, start:0.0802546296296296, stop:0.0802777777777778, text0:"- Yeah? Yeah, there it is.",text1:"- Yes."},{row:1246, start:0.0802893518518519, stop:0.0803009259259259, text0:"Fuck!",text1:""},{row:1247, start:0.0805208333333333, stop:0.0805439814814815, text0:"TIA, huh, Danny?",text1:""},{row:1248, start:0.0805555555555556, stop:0.0805787037037037, text0:"TIA.",text1:""},{row:1249, start:0.0806597222222222, stop:0.0806944444444444, text0:"Keep digging, huh?",text1:"They'll be here any second. Come on."},{row:1250, start:0.0807291666666667, stop:0.0807407407407407, text0:"Keep digging.",text1:""},{row:1251, start:0.0808912037037037, stop:0.0809143518518518, text0:"- It had better be there, huh?",text1:"- Yes, yes."},{row:1252, start:0.0809490740740741, stop:0.0809722222222222, text0:"Have you got it?",text1:""},{row:1253, start:0.0809837962962963, stop:0.0810300925925926, text0:"- Have you got it, huh?",text1:"- Yes, got it. Oh, yes."},{row:1254, start:0.0810416666666667, stop:0.0810532407407407, text0:"Oh, yes.",text1:""},{row:1255, start:0.081087962962963, stop:0.081099537037037, text0:"Solomon.",text1:""},{row:1256, start:0.0811805555555555, stop:0.0812152777777778, text0:"Dia, what are you doing?",text1:""},{row:1257, start:0.0812152777777778, stop:0.0812384259259259, text0:"Dia.",text1:""},{row:1258, start:0.0812615740740741, stop:0.0812847222222222, text0:"What are you doing?",text1:""},{row:1259, start:0.0813425925925926, stop:0.0813541666666667, text0:"Of the proud Mende tribe.",text1:""},{row:1260, start:0.0814236111111111, stop:0.0814814814814815, text0:"You are a good boy",text1:"who loves soccer and school."},{row:1261, start:0.0815046296296296, stop:0.0815509259259259, text0:"Your mother loves you so much.",text1:""},{row:1262, start:0.0815740740740741, stop:0.0816087962962963, text0:"She waits by the fire making plantains...",text1:""},{row:1263, start:0.0816087962962963, stop:0.0816435185185185, text0:"...and red palm oil stew",text1:"with your sister N'Yanda..."},{row:1264, start:0.0816666666666667, stop:0.0816898148148148, text0:"...and the new baby.",text1:""},{row:1265, start:0.0817939814814815, stop:0.0818171296296296, text0:"The cows wait for you.",text1:""},{row:1266, start:0.0818287037037037, stop:0.0818865740740741, text0:"And Babu, the wild dog",text1:"who minds no one but you."},{row:1267, start:0.0819212962962963, stop:0.0819560185185185, text0:"I know they made you do bad things...",text1:""},{row:1268, start:0.0819560185185185, stop:0.0819907407407407, text0:"...but you are not a bad boy.",text1:""},{row:1269, start:0.082025462962963, stop:0.0820601851851852, text0:"I am your father...",text1:""},{row:1270, start:0.0820717592592593, stop:0.0821064814814815, text0:"...who loves you.",text1:""},{row:1271, start:0.0821180555555556, stop:0.0821643518518518, text0:"And you will come home with me",text1:"and be my son again."},{row:1272, start:0.0824537037037037, stop:0.0824768518518518, text0:"Solomon, we've got to move, huh?",text1:""},{row:1273, start:0.0824884259259259, stop:0.0825, text0:"We've got to--",text1:""},{row:1274, start:0.0825347222222222, stop:0.0825694444444444, text0:"Go, go! Move!",text1:""},{row:1275, start:0.0826041666666667, stop:0.0826157407407407, text0:"Move. Move.",text1:""},{row:1276, start:0.0826157407407407, stop:0.0826388888888889, text0:"Move, move, move!",text1:""},{row:1277, start:0.0826388888888889, stop:0.0826736111111111, text0:"Move! Move! Move!",text1:""},{row:1278, start:0.0826967592592593, stop:0.0827083333333333, text0:"How far?",text1:""},{row:1279, start:0.0827083333333333, stop:0.0827314814814815, text0:"There's an airstrip",text1:"at the top of that gorge."},{row:1280, start:0.0827314814814815, stop:0.0827546296296296, text0:"A plane will meet us, huh?",text1:""},{row:1281, start:0.0827662037037037, stop:0.0827893518518519, text0:"Hey, stop. Stop.",text1:""},{row:1282, start:0.0828125, stop:0.0828587962962963, text0:"Give it to me.",text1:"Come on, give it to me, huh?"},{row:1283, start:0.0829513888888889, stop:0.0829861111111111, text0:"Keep moving. They'll be tracking us.",text1:""},{row:1284, start:0.0830555555555556, stop:0.0830787037037037, text0:"Nabil, where are you?",text1:""},{row:1285, start:0.0830902777777778, stop:0.083125, text0:"I'm nearly there.",text1:"Tell me what's happening."},{row:1286, start:0.083125, stop:0.0831712962962963, text0:"We're about 10 K's out, huh?",text1:"We've got it, my friend."},{row:1287, start:0.0831712962962963, stop:0.0831944444444444, text0:"- Who is we?",text1:"- The fisherman and his son."},{row:1288, start:0.0831944444444444, stop:0.0832060185185185, text0:"Lose them.",text1:""},{row:1289, start:0.0833796296296296, stop:0.0834027777777778, text0:"It's at the top of that ridge there, huh?",text1:""},{row:1290, start:0.0834027777777778, stop:0.0834143518518519, text0:"You must stop and rest.",text1:""},{row:1291, start:0.0834143518518519, stop:0.0834490740740741, text0:"What? And give up the damn stone?",text1:""},{row:1292, start:0.0834490740740741, stop:0.0834722222222222, text0:"Keep moving. Come on.",text1:""},{row:1293, start:0.0838773148148148, stop:0.083900462962963, text0:"Come. Come, come.",text1:""},{row:1294, start:0.0844560185185185, stop:0.0844791666666667, text0:"I can't. Stop.",text1:""},{row:1295, start:0.0844791666666667, stop:0.0845023148148148, text0:"- Okay.",text1:"- Stop."},{row:1296, start:0.084837962962963, stop:0.084849537037037, text0:"Come on.",text1:""},{row:1297, start:0.084849537037037, stop:0.0848842592592593, text0:"No! God, no. Stop.",text1:""},{row:1298, start:0.0848958333333333, stop:0.0849305555555556, text0:"Christ. No more. No more.",text1:""},{row:1299, start:0.0854398148148148, stop:0.085462962962963, text0:"Take it, huh?",text1:""},{row:1300, start:0.0854861111111111, stop:0.0855324074074074, text0:"- Mr. Archer--",text1:"- Take it, take it."},{row:1301, start:0.0856134259259259, stop:0.085625, text0:"I thought you would steal it from me.",text1:""},{row:1302, start:0.0856481481481481, stop:0.0856944444444444, text0:"Yeah, yeah, it occurred to me, huh?",text1:""},{row:1303, start:0.0858217592592593, stop:0.0858333333333333, text0:"Listen...",text1:""},{row:1304, start:0.0858680555555556, stop:0.0859027777777778, text0:"...this is Maddy's card, huh?",text1:""},{row:1305, start:0.0859027777777778, stop:0.0859259259259259, text0:"You call her when",text1:"you get to Conakry, all right?"},{row:1306, start:0.0859722222222222, stop:0.0859953703703704, text0:"And don't trust that pilot for a second.",text1:""},{row:1307, start:0.0859953703703704, stop:0.0860416666666667, text0:"You point this at his head",text1:"if he fucks around, all right?"},{row:1308, start:0.0860416666666667, stop:0.0860532407407407, text0:"I can carry you.",text1:""},{row:1309, start:0.0862037037037037, stop:0.0862268518518519, text0:"You take your boy home, huh?",text1:""},{row:1310, start:0.0862847222222222, stop:0.0863078703703704, text0:"You take him home, huh?",text1:""},{row:1311, start:0.0863657407407407, stop:0.0863888888888889, text0:"- Go! Go!",text1:"- Go. Come on."},{row:1312, start:0.086400462962963, stop:0.086412037037037, text0:"Go on!",text1:""},{row:1313, start:0.0864699074074074, stop:0.0864930555555555, text0:"Come on. Come on.",text1:""},{row:1314, start:0.0865740740740741, stop:0.0865972222222222, text0:"Archer, you're a dead man!",text1:""},{row:1315, start:0.0866087962962963, stop:0.0866203703703704, text0:"Yeah, yeah.",text1:""},{row:1316, start:0.0869791666666667, stop:0.0870023148148148, text0:"Dia.",text1:""},{row:1317, start:0.0872453703703704, stop:0.0872569444444444, text0:"Hello. Maddy Bowen.",text1:""},{row:1318, start:0.0872569444444444, stop:0.0872800925925926, text0:"Yeah.",text1:""},{row:1319, start:0.0872800925925926, stop:0.0873032407407407, text0:"Thought I'd never call, huh?",text1:""},{row:1320, start:0.0873148148148148, stop:0.0873611111111111, text0:"And I'm so glad you did.",text1:"Excuse me."},{row:1321, start:0.0873842592592593, stop:0.0873958333333333, text0:"When am I gonna see you?",text1:""},{row:1322, start:0.0874074074074074, stop:0.0874768518518518, text0:"Maddy, I want you to do me",text1:"one more favor, huh?"},{row:1323, start:0.0874884259259259, stop:0.0875347222222222, text0:"I want you to go meet Solomon",text1:"in Conakry."},{row:1324, start:0.0875347222222222, stop:0.0875694444444444, text0:"In Guinea?",text1:"Why do you want me to go to Guinea?"},{row:1325, start:0.0876041666666667, stop:0.0876273148148148, text0:"We found his son...",text1:""},{row:1326, start:0.0876388888888889, stop:0.0876851851851852, text0:"...but he's gonna need some help,",text1:"you understand?"},{row:1327, start:0.0876851851851852, stop:0.0876967592592593, text0:"Maddy?",text1:""},{row:1328, start:0.0877314814814815, stop:0.0877546296296296, text0:"You're hurt.",text1:""},{row:1329, start:0.0877662037037037, stop:0.0877777777777778, text0:"Are you hurt?",text1:""},{row:1330, start:0.0877777777777778, stop:0.0878125, text0:"Yeah, well, I've got",text1:"a little problem here."},{row:1331, start:0.0878703703703704, stop:0.0879050925925926, text0:"Okay. You--",text1:"You tell me where you are."},{row:1332, start:0.0879513888888889, stop:0.087962962962963, text0:"Archer?",text1:""},{row:1333, start:0.0881018518518519, stop:0.0881597222222222, text0:"I'm looking at an incredible view",text1:"right now."},{row:1334, start:0.0881712962962963, stop:0.0881944444444445, text0:"I wish you were here, Maddy.",text1:""},{row:1335, start:0.0881944444444445, stop:0.0882291666666667, text0:"Okay, then I'm coming to be with you.",text1:"You just tell me where you are."},{row:1336, start:0.088275462962963, stop:0.088287037037037, text0:"I don't think so.",text1:""},{row:1337, start:0.088287037037037, stop:0.0883217592592593, text0:"Are you still in Kono? Because I can",text1:"get someone there to help you."},{row:1338, start:0.0883333333333333, stop:0.0883796296296296, text0:"Maddy, you find someplace safe",text1:"for the boy, all right?"},{row:1339, start:0.0883796296296296, stop:0.0884143518518519, text0:"And keep him out of sight.",text1:""},{row:1340, start:0.0884143518518519, stop:0.0884490740740741, text0:"And get Solomon to London.",text1:""},{row:1341, start:0.0884837962962963, stop:0.0885185185185185, text0:"He's bringing something with him.",text1:""},{row:1342, start:0.0885185185185185, stop:0.0885416666666667, text0:"But he's gonna need your help.",text1:""},{row:1343, start:0.0885648148148148, stop:0.0885763888888889, text0:"Why aren't you bringing it yourself?",text1:""},{row:1344, start:0.088599537037037, stop:0.0886574074074074, text0:"I'm saying it's a real story now.",text1:""},{row:1345, start:0.0886689814814815, stop:0.0886921296296296, text0:"And you can write the hell out of it.",text1:""},{row:1346, start:0.0887731481481481, stop:0.0887962962962963, text0:"I'm really happy I met you.",text1:""},{row:1347, start:0.0888194444444445, stop:0.0888310185185185, text0:"You know that?",text1:""},{row:1348, start:0.0888657407407407, stop:0.0888888888888889, text0:"Yeah, I'm...",text1:""},{row:1349, start:0.088912037037037, stop:0.0889467592592593, text0:"I'm really happy I met you too.",text1:""},{row:1350, start:0.0889467592592593, stop:0.0889930555555555, text0:"And I wish I could be there with you.",text1:""},{row:1351, start:0.0890046296296296, stop:0.0890277777777778, text0:"That's all right.",text1:""},{row:1352, start:0.0890625, stop:0.0890856481481481, text0:"I'm exactly where I'm supposed to be.",text1:""},{row:1353, start:0.0897106481481482, stop:0.0897337962962963, text0:"When we get to the stairs,",text1:"you just keep walking..."},{row:1354, start:0.0897337962962963, stop:0.0897569444444444, text0:"...and I'll wait for you here.",text1:""},{row:1355, start:0.0897685185185185, stop:0.0897800925925926, text0:"You're not coming.",text1:""},{row:1356, start:0.0897800925925926, stop:0.0898263888888889, text0:"I'm not here. Good luck.",text1:""},{row:1357, start:0.0900694444444444, stop:0.0901041666666667, text0:"You must understand, Mr. Vandy...",text1:""},{row:1358, start:0.0901041666666667, stop:0.0901736111111111, text0:"...that your diamond could have ended up",text1:"nowhere else but with us."},{row:1359, start:0.0902199074074074, stop:0.0902546296296296, text0:"- It is not enough.",text1:"- I assure you, 2 million pounds..."},{row:1360, start:0.0902546296296296, stop:0.0903009259259259, text0:"...is more than anyone else will offer you",text1:"for that stone under the circumstances."},{row:1361, start:0.0903009259259259, stop:0.0903240740740741, text0:"I want what was promised to me",text1:"by Mr. Archer."},{row:1362, start:0.0903703703703704, stop:0.0904050925925926, text0:"- What was promised to you?",text1:"- My family."},{row:1363, start:0.0904050925925926, stop:0.0904282407407407, text0:"When they are here,",text1:"you will get the stone."},{row:1364, start:0.0904398148148148, stop:0.0904513888888889, text0:"I will have the money too.",text1:""},{row:1365, start:0.090462962962963, stop:0.0904861111111111, text0:"Well, may I see what I'm buying?",text1:""},{row:1366, start:0.0904861111111111, stop:0.0905208333333333, text0:"You will get the stone",text1:"once my family is here."},{row:1367, start:0.0905324074074074, stop:0.0905439814814815, text0:"How do I even know you've got it?",text1:""},{row:1368, start:0.092025462962963, stop:0.0920601851851852, text0:"Can you comment on the diamonds",text1:"mined in Sierra Leone?"},{row:1369, start:0.0920601851851852, stop:0.0921064814814815, text0:"I'm not going to comment",text1:"on some sensationalist magazine article."},{row:1370, start:0.0921064814814815, stop:0.0921412037037037, text0:"- What about Liberia?",text1:"- No comment. No comment."},{row:1371, start:0.0921412037037037, stop:0.0921759259259259, text0:"- Mr. Van De Kaap--",text1:"- That's all I have to say."},{row:1372, start:0.0921759259259259, stop:0.0921875, text0:"The natural resources",text1:"of a country..."},{row:1373, start:0.0922106481481482, stop:0.0922453703703704, text0:"...are the property of its people.",text1:"They are not ours to steal..."},{row:1374, start:0.0922569444444444, stop:0.0923263888888889, text0:"...or exploit in the name of our comfort,",text1:"our corporations, or our consumerism."},{row:1375, start:0.0924421296296296, stop:0.0924652777777778, text0:"They're ready for you, sir.",text1:""},{row:1376, start:0.0924768518518519, stop:0.0925115740740741, text0:"The Third World is not a world apart.",text1:""},{row:1377, start:0.0925115740740741, stop:0.0925694444444444, text0:"And the witness you will hear today",text1:"speaks on its behalf."},{row:1378, start:0.0925694444444444, stop:0.0926157407407407, text0:"Let us hear the voice of that world.",text1:"Let us learn from that voice."},{row:1379, start:0.0926157407407407, stop:0.092650462962963, text0:"And let us ignore it no more.",text1:""},{row:1380, start:0.092662037037037, stop:0.0926967592592593, text0:"Ladies and gentlemen,",text1:"Mr. Solomon Vandy."}]}); 

